  }\n  \n  /* üìä PERFECTLY CENTERED STATS PANEL */\n  .session-info-panel {\n    width: 100%;\n    max-width: 900px;\n    background: var(--surface-secondary);\n    border-radius: var(--radius-lg);\n    padding: var(--space-4);\n    border: 1px solid var(--border-light);\n    box-shadow: var(--shadow-md);\n    margin: var(--space-2) auto;\n    animation: slideInUp 0.4s ease-out;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n  }\n  \n  @keyframes slideInUp {\n    from {\n      opacity: 0;\n      transform: translateY(20px);\n    }\n    to {\n      opacity: 1;\n      transform: translateY(0);\n    }\n  }\n  \n  /* üïê STUNNING CIRCULAR TIMER - Visual centerpiece */\n  .session-timer-fixed {\n    width: 100%;\n    max-width: 900px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    margin: var(--space-6) auto;\n    z-index: 30;\n  }\n  \n  .circular-timer {\n    position: relative;\n    width: 140px;\n    height: 140px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    margin: 0 auto;\n    animation: timerPulse 2s ease-in-out infinite;\n  }\n  \n  @keyframes timerPulse {\n    0%, 100% { transform: scale(1); }\n    50% { transform: scale(1.02); }\n  }\n  \n  .circular-timer svg {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    transform: rotate(-90deg);\n  }\n  \n  .circular-timer .timer-bg {\n    fill: none;\n    stroke: rgba(var(--border-primary-rgb), 0.2);\n    stroke-width: 8;\n    stroke-linecap: round;\n  }\n  \n  .circular-timer .timer-progress {\n    fill: none;\n    stroke-width: 8;\n    stroke-linecap: round;\n    transition: all 0.5s ease-in-out;\n    filter: drop-shadow(0 0 8px currentColor);\n  }\n  \n  /* Dynamic color based on time remaining */\n  .circular-timer .timer-progress.safe {\n    stroke: var(--color-success);\n    animation: progressGlow 3s ease-in-out infinite;\n  }\n  \n  .circular-timer .timer-progress.warning {\n    stroke: var(--color-warning);\n    animation: progressWarning 2s ease-in-out infinite;\n  }\n  \n  .circular-timer .timer-progress.critical {\n    stroke: var(--color-error);\n    animation: progressCritical 1s ease-in-out infinite;\n  }\n  \n  @keyframes progressGlow {\n    0%, 100% { filter: drop-shadow(0 0 8px currentColor); }\n    50% { filter: drop-shadow(0 0 16px currentColor); }\n  }\n  \n  @keyframes progressWarning {\n    0%, 100% { filter: drop-shadow(0 0 12px currentColor); }\n    50% { filter: drop-shadow(0 0 20px currentColor); }\n  }\n  \n  @keyframes progressCritical {\n    0%, 100% { \n      filter: drop-shadow(0 0 16px currentColor);\n      transform: scale(1);\n    }\n    50% { \n      filter: drop-shadow(0 0 24px currentColor);\n      transform: scale(1.05);\n    }\n  }\n  \n  /* Central timer display */\n  .circular-timer .timer-content {\n    position: relative;\n    text-align: center;\n    z-index: 10;\n    background: var(--surface-primary);\n    border-radius: 50%;\n    width: 100px;\n    height: 100px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    box-shadow: \n      0 4px 20px rgba(0, 0, 0, 0.1),\n      inset 0 1px 0 rgba(255, 255, 255, 0.2);\n    border: 3px solid var(--surface-secondary);\n  }\n  \n  .circular-timer .timer-time {\n    font-size: var(--text-xl);\n    font-weight: var(--font-bold);\n    color: var(--text-primary);\n    line-height: 1;\n    margin-bottom: var(--space-1);\n  }\n  \n  .circular-timer .timer-label {\n    font-size: var(--text-xs);\n    color: var(--text-secondary);\n    text-transform: uppercase;\n    letter-spacing: 0.5px;\n    font-weight: var(--font-semibold);\n  }\n  \n  .circular-timer .timer-icon {\n    position: absolute;\n    top: -8px;\n    right: -8px;\n    background: var(--color-primary);\n    color: white;\n    border-radius: 50%;\n    width: 32px;\n    height: 32px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: var(--text-sm);\n    box-shadow: 0 4px 12px rgba(var(--color-primary-rgb), 0.4);\n    animation: iconSpin 20s linear infinite;\n  }\n  \n  @keyframes iconSpin {\n    from { transform: rotate(0deg); }\n    to { transform: rotate(360deg); }\n  }\n  \n  /* Critical state enhancements */\n  .circular-timer.critical .timer-content {\n    border-color: var(--color-error);\n    box-shadow: \n      0 4px 20px rgba(var(--color-error-rgb), 0.3),\n      0 0 0 4px rgba(var(--color-error-rgb), 0.1),\n      inset 0 1px 0 rgba(255, 255, 255, 0.2);\n  }\n  \n  .circular-timer.critical .timer-time {\n    color: var(--color-error);\n    animation: timeFlash 1s ease-in-out infinite;\n  }\n  \n  @keyframes timeFlash {\n    0%, 100% { opacity: 1; }\n    50% { opacity: 0.7; }\n  }\n  \n  /* Warning state */\n  .circular-timer.warning .timer-content {\n    border-color: var(--color-warning);\n  }\n  \n  .circular-timer.warning .timer-time {\n    color: var(--color-warning);\n  }\n  \n  /* Hide timer from stats panel */\n  .session-info-panel .session-timer {\n    display: none;\n  }\n  \n  /* üìä PERFECTLY CENTERED STATS GRID */\n  .session-info-panel .stats-grid {\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n    gap: var(--space-4);\n    margin: 0 auto;\n    width: 100%;\n    max-width: 600px;\n    justify-items: center;\n  }\n  \n  .session-info-panel .stat-item {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: var(--space-4);\n    background: var(--surface-primary);\n    border: 1px solid var(--border-light);\n    border-radius: var(--radius-md);\n    transition: all var(--transition-normal);\n    box-shadow: var(--shadow-sm);\n  }\n  \n  .session-info-panel .stat-item:hover {\n    transform: translateY(-2px);\n    border-color: var(--color-primary);\n    box-shadow: var(--shadow-lg);\n  }\n  \n  /* Compact circular progress */\n  .circular-progress {\n    position: relative;\n    width: 60px;\n    height: 60px;\n    margin-bottom: var(--space-2);\n  }\n  \n  .circular-progress svg {\n    width: 100%;\n    height: 100%;\n    transform: rotate(-90deg);\n  }\n  \n  .circular-progress .bg-circle {\n    fill: none;\n    stroke: rgba(var(--border-primary-rgb), 0.2);\n    stroke-width: 4;\n  }\n  \n  .circular-progress .progress-circle {\n    fill: none;\n    stroke-width: 4;\n    stroke-linecap: round;\n    transition: stroke-dasharray 0.5s ease-in-out;\n  }\n  \n  .circular-progress .progress-circle.questions {\n    stroke: var(--color-primary);\n    stroke-dasharray: 163.4; /* 2 * œÄ * 26 */\n    stroke-dashoffset: 0;\n    animation: drawCircle 0.8s ease-out;\n  }\n  \n  .circular-progress .progress-circle.correct {\n    stroke: var(--color-success);\n    stroke-dasharray: 163.4;\n    stroke-dashoffset: 0;\n    animation: drawCircle 1s ease-out;\n  }\n  \n  .circular-progress .progress-circle.xp {\n    stroke: var(--color-accent);\n    stroke-dasharray: 163.4;\n    stroke-dashoffset: 0;\n    animation: drawCircle 1.2s ease-out;\n  }\n  \n  .circular-progress .progress-circle.level {\n    stroke: var(--color-warning);\n    stroke-dasharray: 163.4;\n    stroke-dashoffset: 0;\n    animation: drawCircle 1.4s ease-out;\n  }\n  \n  @keyframes drawCircle {\n    from { stroke-dashoffset: 163.4; }\n    to { stroke-dashoffset: 0; }\n  }\n  \n  /* Center value in circle */\n  .circular-progress .center-value {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    text-align: center;\n  }\n  \n  .session-info-panel .stat-value {\n    font-size: var(--text-lg);\n    font-weight: var(--font-bold);\n    color: var(--text-primary);\n    line-height: 1.2;\n  }\n  \n  .session-info-panel .stat-label {\n    font-size: var(--text-xs);\n    font-weight: var(--font-medium);\n    color: var(--text-secondary);\n    text-align: center;\n    margin-top: var(--space-1);\n    text-transform: uppercase;\n    letter-spacing: 0.025em;\n  }\n\n  /* Add progress indicator to stats */\n  .session-info-panel .stat-item.correct .stat-value {\n    color: var(--color-success);\n  }\n\n  .session-info-panel .stat-item.xp .stat-value {\n    color: var(--color-accent);\n  }\n\n  /* Enhanced accessibility - keyboard navigation */\n  .session-info-panel .stat-item:focus {\n    outline: 2px solid var(--color-primary);\n    outline-offset: 2px;\n  }\n\n  /* Progress tracking visual enhancement */\n  .session-info-panel::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    height: 3px;\n    background: linear-gradient(90deg, var(--color-primary), var(--color-success));\n    border-radius: var(--radius-lg) var(--radius-lg) 0 0;\n    opacity: 0.6;\n  }\n  \n  /* Question areas are now part of main-content-area directly */\n  \n  /* ‚ú® FOCUSED QUESTION SECTION - App-consistent styling */\n  .question-section {\n    padding: var(--space-8);\n    margin: var(--space-4) auto;\n    width: 100%;\n    max-width: 800px;\n    background: var(--surface-primary);\n    border-radius: var(--radius-lg);\n    border: 2px solid var(--color-primary);\n    box-shadow: var(--shadow-xl);\n    position: relative;\n    z-index: 20;\n    transition: all var(--transition-normal);\n    animation: questionFocusIn 0.5s ease-out;\n  }\n  \n  @keyframes questionFocusIn {\n    from {\n      opacity: 0;\n      transform: translateY(20px);\n    }\n    to {\n      opacity: 1;\n      transform: translateY(0);\n    }\n  }\n  \n  .question-section:hover {\n    transform: translateY(-2px);\n    box-shadow: var(--shadow-2xl);\n    border-color: var(--color-primary-hover);\n  }\n  \n  .question-section::before {\n    content: '';\n    position: absolute;\n    top: -2px;\n    left: -2px;\n    right: -2px;\n    bottom: -2px;\n    background: linear-gradient(135deg, var(--color-primary), var(--color-success));\n    border-radius: var(--radius-xl);\n    z-index: -1;\n    opacity: 0.1;\n  }\n  \n  /* üé® CONSISTENT CATEGORY SELECTION */\n  .category-selection {\n    width: 100%;\n    max-width: 700px;\n    margin: 0 auto;\n    padding: var(--space-6);\n    background: var(--surface-secondary);\n    border-radius: var(--radius-lg);\n    border: 1px solid var(--border-light);\n    box-shadow: var(--shadow-md);\n    animation: slideInUp 0.5s ease-out;\n  }\n  \n  .category-selection h3 {\n    text-align: center;\n    margin-bottom: var(--space-4);\n    font-size: var(--text-xl);\n    font-weight: var(--font-bold);\n    color: var(--text-primary);\n  }\n  \n  /* Enhanced category grid */\n  .category-grid {\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\n    gap: var(--space-4);\n  }\n  \n  .category-button {\n    padding: var(--space-4);\n    background: var(--surface-primary);\n    border: 1px solid var(--border-light);\n    border-radius: var(--radius-md);\n    transition: all var(--transition-normal);\n    text-align: center;\n    cursor: pointer;\n    box-shadow: var(--shadow-sm);\n  }\n  \n  .category-button:hover {\n    transform: translateY(-2px);\n    border-color: var(--color-primary);\n    box-shadow: var(--shadow-lg);\n  }\n  \n  .category-button.selected {\n    background: var(--color-primary);\n    color: white;\n    border-color: var(--color-primary);\n    box-shadow: var(--shadow-lg);\n  }\n  \n  @keyframes slideInUp {\n    from {\n      opacity: 0;\n      transform: translateY(30px);\n    }\n    to {\n      opacity: 1;\n      transform: translateY(0);\n    }\n  }\n  \n  /* üöÄ ENHANCED SESSION CONTROLS */\n  .session-controls {\n    width: 100%;\n    max-width: 400px;\n    margin: var(--space-6) auto;\n    text-align: center;\n  }\n  \n  .session-controls .btn {\n    position: relative;\n    padding: var(--space-4) var(--space-8);\n    font-size: var(--text-lg);\n    font-weight: var(--font-bold);\n    border-radius: 50px;\n    transition: all var(--transition-normal);\n    text-transform: none;\n    letter-spacing: 0.5px;\n    overflow: hidden;\n  }\n  \n  .session-controls .btn::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: -100%;\n    width: 100%;\n    height: 100%;\n    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);\n    transition: left 0.6s ease;\n  }\n  \n  .session-controls .btn:hover::before {\n    left: 100%;\n  }\n  \n  .session-controls .btn:hover {\n    transform: translateY(-3px) scale(1.05);\n    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.15);\n  }\n  \n  /* Perfectly centered header styling */\n  .learning-header {\n    padding: var(--space-6);\n    margin: 0 auto var(--space-4) auto;\n    background: var(--surface-secondary);\n    border: 1px solid var(--border-light);\n    border-radius: var(--radius-lg);\n    text-align: center;\n    max-width: 800px;\n    width: 100%;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n  }\n  \n  .learning-header .page-title {\n    font-size: var(--text-3xl);\n    font-weight: var(--font-bold);\n    margin-bottom: var(--space-2);\n    color: var(--text-primary);\n  }\n  \n  .learning-header .page-subtitle {\n    font-size: var(--text-lg);\n    color: var(--text-secondary);\n    opacity: 0.9;\n    line-height: var(--leading-relaxed);\n  }\n}\n\n/* üì± MOBILE OPTIMIZATIONS */\n@media (max-width: 768px) {\n  /* Keep session-info-panel visible but optimize for mobile */\n  .session-info-panel {\n    margin-top: var(--space-3) !important;\n    padding: var(--space-4) !important;\n    max-width: 100% !important;\n  }\n\n  /* Mobile stats - perfectly centered grid */\n  .session-info-panel .stats-grid {\n    grid-template-columns: repeat(2, 1fr) !important;\n    gap: var(--space-4) !important;\n    justify-items: center !important;\n    max-width: 320px !important;\n    margin: 0 auto !important;\n  }\n\n  /* Mobile circular progress - smaller */\n  .circular-progress {\n    width: 50px !important;\n    height: 50px !important;\n  }\n\n  /* Adjust mobile SVG coordinates */\n  .session-info-panel .circular-progress svg circle {\n    cx: 25 !important;\n    cy: 25 !important;\n    r: 20 !important;\n  }\n\n  /* Mobile stroke-dasharray adjustment */\n  .session-info-panel .circular-progress .progress-circle {\n    stroke-dasharray: 125.6 !important; /* 2 * œÄ * 20 */\n  }\n\n  /* Mobile stat items - more compact */\n  .session-info-panel .stat-item {\n    padding: var(--space-3) !important;\n  }\n\n  /* Mobile adjustments for overall layout */\n  .adaptive-learning-container {\n    padding: var(--space-3) !important;\n  }\n  \n  /* üïê Mobile circular timer adjustments */\n  .circular-timer-container {\n    margin: var(--space-4) 0 !important;\n  }\n  \n  .circular-timer {\n    width: 100px !important;\n    height: 100px !important;\n  }\n  \n  .circular-timer svg {\n    width: 100px !important;\n    height: 100px !important;\n  }\n  \n  .circular-timer .timer-content {\n    width: 70px !important;\n    height: 70px !important;\n  }\n  \n  .circular-timer .timer-time {\n    font-size: var(--text-base) !important;\n    line-height: 1.2 !important;\n  }\n  \n  .circular-timer .timer-label {\n    font-size: var(--text-xs) !important;\n  }\n  \n  /* Extra small screens - under 480px */\n  @media (max-width: 480px) {\n    .circular-timer {\n      width: 90px !important;\n      height: 90px !important;\n    }\n    \n    .circular-timer svg {\n      width: 90px !important;\n      height: 90px !important;\n    }\n    \n    .circular-timer .timer-content {\n      width: 60px !important;\n      height: 60px !important;\n    }\n    \n    .circular-timer .timer-time {\n      font-size: var(--text-sm) !important;\n    }\n  }\n  \n  /* Ensure circular timer stays circular on all mobile devices */\n  @media (max-width: 768px) {\n    .circular-timer-container {\n      width: 100% !important;\n      text-align: center !important;\n    }\n    \n    .circular-timer {\n      display: flex !important;\n      border-radius: 50% !important;\n      background: var(--surface-secondary) !important;\n      box-shadow: var(--shadow-lg) !important;\n    }\n    \n    .circular-timer svg {\n      border-radius: 50% !important;\n    }\n    \n    .circular-timer .timer-content {\n      border-radius: 50% !important;\n      background: var(--surface-primary) !important;\n    }\n  }\n  \n  /* Mobile question section adjustments */\n  .question-section {\n    padding: var(--space-5) !important;\n    margin: var(--space-3) auto !important;\n  }\n  \n  /* Mobile category layout */\n  .category-selection {\n    padding: var(--space-5) !important;\n    margin: var(--space-3) 0 !important;\n  }\n  \n  .category-grid {\n    grid-template-columns: 1fr 1fr !important;\n    gap: var(--space-3) !important;\n  }\n  \n  .category-button {\n    padding: var(--space-4) !important;\n    min-height: 80px !important;\n  }\n  \n  /* Mobile session controls */\n  .session-controls {\n    margin: var(--space-4) auto !important;\n    max-width: 320px !important;\n  }\n  \n  .session-controls .btn {\n    width: 100% !important;\n    padding: var(--space-4) var(--space-6) !important;\n    min-height: 50px !important;\n  }\n}\n\n/* Question Focus Enhancements - All devices */\n.question-section {\n  transition: all var(--transition-normal);\n}\n\n.question-section:hover {\n  transform: translateY(-1px);\n  box-shadow: var(--shadow-lg);\n}\n\n/* Progress indicator improvements */\n.session-progress {\n  position: relative;\n  overflow: hidden;\n}\n\n.session-progress::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  height: 2px;\n  background: linear-gradient(90deg, var(--color-primary) 0%, var(--color-success) 100%);\n  transform: scaleX(var(--progress, 0));\n  transform-origin: left;\n  transition: transform var(--transition-slow);\n}\n\n/* Improved feedback animations */\n.answer-feedback {\n  animation: slideInFromBottom 0.3s ease-out;\n}\n\n@keyframes slideInFromBottom {\n  from {\n    opacity: 0;\n    transform: translateY(20px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n/* Enhanced success state */\n.success-animation .question-section {\n  border-color: var(--color-success);\n  box-shadow: 0 0 20px rgba(var(--color-success), 0.3);\n}\n\n/* ‚ö° SUCCESS ANIMATIONS & MICRO-INTERACTIONS */\n.answer-feedback.correct {\n  background: linear-gradient(135deg, var(--color-success), var(--color-accent));\n  color: white;\n  border-radius: var(--radius-xl);\n  box-shadow: 0 8px 25px rgba(var(--color-success-rgb), 0.3);\n  animation: successBounce 0.6s ease-out;\n}\n\n.answer-feedback.incorrect {\n  background: linear-gradient(135deg, var(--color-error), var(--color-warning));\n  color: white;\n  border-radius: var(--radius-xl);\n  box-shadow: 0 8px 25px rgba(var(--color-error-rgb), 0.3);\n  animation: errorShake 0.6s ease-out;\n}\n\n@keyframes successBounce {\n  0% { transform: scale(0.8) translateY(20px); opacity: 0; }\n  60% { transform: scale(1.1) translateY(-5px); opacity: 1; }\n  100% { transform: scale(1) translateY(0); }\n}\n\n@keyframes errorShake {\n  0%, 100% { transform: translateX(0); }\n  25% { transform: translateX(-5px); }\n  75% { transform: translateX(5px); }\n}\n\n/* Enhanced XP celebration */\n.instant-xp-popup {\n  position: fixed;\n  top: 20%;\n  left: 50%;\n  transform: translateX(-50%);\n  background: linear-gradient(135deg, var(--color-success), var(--color-accent));\n  color: white;\n  padding: var(--space-4) var(--space-6);\n  border-radius: 50px;\n  box-shadow: 0 12px 30px rgba(var(--color-success-rgb), 0.4);\n  z-index: 100;\n  animation: xpCelebration 2s ease-out forwards;\n}\n\n@keyframes xpCelebration {\n  0% { \n    opacity: 0; \n    transform: translateX(-50%) scale(0.5) translateY(50px); \n  }\n  20% { \n    opacity: 1; \n    transform: translateX(-50%) scale(1.2) translateY(-10px); \n  }\n  40% { \n    transform: translateX(-50%) scale(1) translateY(0); \n  }\n  80% { \n    opacity: 1; \n    transform: translateX(-50%) scale(1) translateY(0); \n  }\n  100% { \n    opacity: 0; \n    transform: translateX(-50%) scale(0.8) translateY(-30px); \n  }\n}\n\n/* Accessibility improvements */\n@media (prefers-reduced-motion: reduce) {\n  .question-section,\n  .answer-feedback,\n  .session-info-panel,\n  .category-button,\n  .instant-xp-popup,\n  .adaptive-learning-container::before {\n    animation: none !important;\n    transition: none !important;\n  }\n  \n  .question-section:hover,\n  .category-button:hover,\n  .session-controls .btn:hover {\n    transform: none !important;\n  }\n}\n\n/* High contrast mode enhancements */\n@media (prefers-contrast: high) {\n  .question-section {\n    border-width: 3px;\n  }\n  \n  .stat-item {\n    border-width: 2px;\n  }\n  \n  .timer-display {\n    border-width: 2px;\n  }\n}\n/*# sourceMappingURL=data:application/json;base64, */"],["STYLE",{},"\n/* üéØ Clean Stats Container - Timer Central Focus */\n.clean-stats-container {\n  width: 100%;\n  padding: 1.5rem 1rem;\n  background: linear-gradient(135deg, \n    rgba(59, 130, 246, 0.1) 0%, \n    rgba(147, 51, 234, 0.1) 50%, \n    rgba(239, 68, 68, 0.1) 100%);\n  border-radius: 20px;\n  margin-bottom: 2rem;\n  backdrop-filter: blur(10px);\n  border: 1px solid rgba(255, 255, 255, 0.2);\n  box-shadow: 0 8px 32px rgba(0, 0, 0, 0.1);\n}\n\n.stats-layout {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  max-width: 600px;\n  margin: 0 auto;\n  gap: 2rem;\n}\n\n/* üåü Side Stats (XP & Success) */\n.side-stat {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  min-width: 120px;\n  padding: 1rem;\n  background: rgba(255, 255, 255, 0.1);\n  border-radius: 16px;\n  border: 2px solid rgba(255, 255, 255, 0.2);\n  transition: all 0.3s ease;\n  backdrop-filter: blur(5px);\n}\n\n.side-stat:hover {\n  transform: translateY(-3px);\n  box-shadow: 0 12px 25px rgba(0, 0, 0, 0.15);\n  border-color: rgba(255, 255, 255, 0.4);\n  background: rgba(255, 255, 255, 0.15);\n}\n\n/* XP Icon */\n.stat-icon-large {\n  width: 50px;\n  height: 50px;\n  border-radius: 50%;\n  background: linear-gradient(135deg, #fbbf24 0%, #f59e0b 100%);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin-bottom: 0.75rem;\n  box-shadow: 0 4px 15px rgba(251, 191, 36, 0.3);\n}\n\n.stat-icon-large .icon {\n  font-size: 1.75rem;\n  filter: drop-shadow(0 2px 4px rgba(0, 0, 0, 0.3));\n}\n\n/* Stat Values & Labels */\n.stat-value-large {\n  font-size: 1.75rem;\n  font-weight: 800;\n  color: #ffffff;\n  text-shadow: 0 2px 8px rgba(0, 0, 0, 0.3);\n  margin-bottom: 0.25rem;\n}\n\n.stat-label-clean {\n  font-size: 0.8rem;\n  color: rgba(255, 255, 255, 0.9);\n  font-weight: 600;\n  text-transform: uppercase;\n  letter-spacing: 1px;\n  text-align: center;\n}\n\n/* ‚è∞ Central Timer - Hero Element */\n.center-timer {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  position: relative;\n  z-index: 2;\n}\n\n.timer-circle-large {\n  position: relative;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.timer-svg-large {\n  filter: drop-shadow(0 8px 20px rgba(0, 0, 0, 0.2));\n}\n\n.timer-content-large {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  text-align: center;\n  z-index: 3;\n}\n\n.timer-time-large {\n  font-size: 2rem;\n  font-weight: 900;\n  color: #ffffff;\n  text-shadow: 0 3px 10px rgba(0, 0, 0, 0.4);\n  font-family: 'Courier New', monospace;\n  letter-spacing: 2px;\n}\n\n.timer-label-large {\n  font-size: 0.7rem;\n  color: rgba(255, 255, 255, 0.8);\n  font-weight: 600;\n  text-transform: uppercase;\n  letter-spacing: 1px;\n  margin-top: 0.25rem;\n}\n\n/* Timer Progress Colors */\n.timer-progress-large.safe {\n  stroke: #10b981;\n  filter: drop-shadow(0 0 8px rgba(16, 185, 129, 0.6));\n}\n\n.timer-progress-large.warning {\n  stroke: #f59e0b;\n  filter: drop-shadow(0 0 8px rgba(245, 158, 11, 0.6));\n}\n\n.timer-progress-large.critical {\n  stroke: #ef4444;\n  filter: drop-shadow(0 0 12px rgba(239, 68, 68, 0.8));\n  animation: criticalPulse 1s ease-in-out infinite;\n}\n\n@keyframes criticalPulse {\n  0%, 100% { \n    opacity: 1; \n    stroke-width: 8;\n  }\n  50% { \n    opacity: 0.7; \n    stroke-width: 10;\n  }\n}\n\n/* Critical Time Pulse Effect */\n.pulse-effect {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  width: 140px;\n  height: 140px;\n  border: 3px solid #ef4444;\n  border-radius: 50%;\n  animation: expandPulse 2s ease-out infinite;\n  pointer-events: none;\n  z-index: 1;\n}\n\n@keyframes expandPulse {\n  0% {\n    width: 120px;\n    height: 120px;\n    opacity: 1;\n  }\n  100% {\n    width: 160px;\n    height: 160px;\n    opacity: 0;\n  }\n}\n\n/* üìä Success Rate Circle */\n.circular-progress-large {\n  position: relative;\n  margin-bottom: 0.75rem;\n}\n\n.center-value-large {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  text-align: center;\n}\n\n/* üì± Mobile Responsive Design */\n@media (max-width: 768px) {\n  .clean-stats-container {\n    padding: 1rem 0.75rem;\n  }\n  \n  .stats-layout {\n    gap: 1rem;\n    max-width: 100%;\n  }\n  \n  .side-stat {\n    min-width: 90px;\n    padding: 0.75rem;\n  }\n  \n  .stat-icon-large {\n    width: 40px;\n    height: 40px;\n  }\n  \n  .stat-icon-large .icon {\n    font-size: 1.5rem;\n  }\n  \n  .stat-value-large {\n    font-size: 1.5rem;\n  }\n  \n  .timer-svg-large {\n    width: 100px;\n    height: 100px;\n  }\n  \n  .timer-time-large {\n    font-size: 1.5rem;\n  }\n  \n  .circular-progress-large svg {\n    width: 50px;\n    height: 50px;\n  }\n}\n\n@media (max-width: 580px) {\n  .stats-layout {\n    flex-direction: column;\n    gap: 1.5rem;\n  }\n  \n  .center-timer {\n    order: -1; /* Timer goes to top on mobile */\n  }\n  \n  .side-stat {\n    min-width: 140px;\n  }\n  \n  .stats-layout {\n    align-items: center;\n  }\n}\n\n@media (max-width: 400px) {\n  .side-stat {\n    min-width: 120px;\n    padding: 0.5rem;\n  }\n  \n  .stat-value-large {\n    font-size: 1.25rem;\n  }\n  \n  .timer-svg-large {\n    width: 90px;\n    height: 90px;\n  }\n  \n  .timer-time-large {\n    font-size: 1.25rem;\n  }\n}\n"],["STYLE",{},".app-header {\n  position: sticky;\n  top: 0;\n  z-index: 1000;\n  background: var(--bg-card, linear-gradient(135deg, #ffffff 0%, #f8fafc 100%));\n  border-bottom: 2px solid var(--border-light, #e5e7eb);\n  backdrop-filter: blur(10px);\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.08);\n  transition: all 0.3s ease;\n}\n\n.header-content {\n  max-width: 1400px;\n  margin: 0 auto;\n  padding: 12px 24px;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  gap: 20px;\n  min-height: 60px;\n  box-sizing: border-box;\n  flex-wrap: wrap;\n  overflow: visible;\n  position: relative;\n}\n\n/* FLAT STRUCTURE - Logo */\n.header-logo {\n  display: flex;\n  align-items: center;\n  flex-shrink: 0;\n  min-width: 0;\n  gap: 12px;\n  order: 1;\n}\n\n.logo-icon {\n  font-size: 24px;\n  animation: bounce 2s infinite;\n}\n\n@keyframes bounce {\n  0%, 20%, 50%, 80%, 100% { transform: translateY(0); }\n  40% { transform: translateY(-6px); }\n  60% { transform: translateY(-3px); }\n}\n\n.app-title {\n  font-size: var(--text-xl);\n  font-weight: var(--font-bold);\n  line-height: var(--leading-tight);\n  letter-spacing: var(--tracking-tight);\n  color: var(--color-primary);\n  margin: 0;\n  text-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  max-width: 100%;\n}\n\n/* FLAT STRUCTURE - User Info */\n.header-user {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  background: var(--bg-card, #ffffff);\n  padding: 8px 16px;\n  border-radius: 20px;\n  border: 1px solid var(--border-light, #f3f4f6);\n  box-shadow: 0 2px 6px rgba(0, 0, 0, 0.05);\n  order: 2;\n  flex-shrink: 1;\n}\n\n.user-role-icon {\n  font-size: 20px;\n}\n\n.user-details {\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n}\n\n.user-name {\n  font-weight: var(--font-semibold);\n  font-size: var(--text-lg);\n  color: var(--color-primary);\n  line-height: var(--leading-snug);\n  font-family: var(--font-sans);\n  letter-spacing: var(--tracking-normal);\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  max-width: 100%;\n}\n\n/* Debug: Explicit color scheme overrides */\n:root[data-color=\"purple\"] .user-name {\n  color: #8B5FBF !important;\n}\n\n:root[data-color=\"blue\"] .user-name {\n  color: #0084FF !important;\n}\n\n:root[data-color=\"green\"] .user-name {\n  color: #00D25B !important;\n}\n\n:root[data-color=\"red\"] .user-name {\n  color: #FF3B30 !important;\n}\n\n:root[data-color=\"orange\"] .user-name {\n  color: #FF9500 !important;\n}\n\n:root[data-color=\"cyber\"] .user-name {\n  color: #00FF88 !important;\n}\n\n:root[data-color=\"ocean\"] .user-name {\n  color: #0084FF !important;\n}\n\n:root[data-color=\"sunset\"] .user-name {\n  color: #FF6B35 !important;\n}\n\n.user-role {\n  font-size: 12px;\n  color: var(--text-secondary, #6b7280);\n  font-weight: var(--font-medium);\n  text-transform: capitalize;\n}\n\n/* FLAT STRUCTURE - Direct Theme Buttons */\n.header-theme-btn {\n  padding: 6px 8px;\n  border: 1px solid var(--border-light, #e5e7eb);\n  background: var(--bg-secondary, #f9fafb);\n  border-radius: 8px;\n  cursor: pointer;\n  font-size: 16px;\n  transition: all 0.2s ease;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-width: 36px;\n  height: 36px;\n  order: 3;\n  flex-shrink: 0;\n  margin: 0 2px;\n}\n\n.header-theme-btn:hover {\n  background: var(--bg-hover, #ede9fe);\n  border-color: var(--color-primary, #6366f1);\n  transform: translateY(-1px);\n  box-shadow: 0 2px 4px rgba(99, 102, 241, 0.2);\n}\n\n.header-theme-btn.active {\n  background: var(--color-primary, #6366f1);\n  border-color: var(--color-primary, #6366f1);\n  color: white;\n  box-shadow: 0 2px 8px rgba(99, 102, 241, 0.3);\n  transform: translateY(-1px);\n}\n\n/* FLAT STRUCTURE - Direct Color Trigger */\n.header-color-trigger {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  padding: 6px 12px;\n  background: var(--bg-secondary, #f9fafb);\n  border: 1px solid var(--border-light, #e5e7eb);\n  border-radius: 8px;\n  cursor: pointer;\n  transition: all 0.2s ease;\n  font-size: 14px;\n  white-space: nowrap;\n  order: 4;\n  flex-shrink: 0;\n  min-width: 100px;\n  height: 36px;\n}\n\n.header-color-trigger:hover {\n  background: var(--bg-hover, #ede9fe);\n  border-color: var(--color-primary, #6366f1);\n}\n\n/* FLAT STRUCTURE - Direct Logout Button */\n.header-logout-btn {\n  padding: 6px 12px;\n  background: var(--bg-secondary, #f1f5f9);\n  color: var(--text-secondary, #64748b);\n  border: 1px solid var(--border-light, #e2e8f0);\n  border-radius: 8px;\n  font-size: 14px;\n  font-weight: var(--font-medium, 500);\n  cursor: pointer;\n  transition: all 0.2s ease;\n  display: flex;\n  align-items: center;\n  gap: 4px;\n  white-space: nowrap;\n  order: 5;\n  flex-shrink: 0;\n  height: 36px;\n}\n\n.header-logout-btn:hover {\n  background: var(--bg-hover, #e2e8f0);\n  color: var(--text-primary, #1e293b);\n  border-color: var(--border-medium, #cbd5e1);\n  transform: translateY(-1px);\n  box-shadow: 0 2px 4px rgba(100, 116, 139, 0.1);\n}\n\n/* ===== INNOVATIVE MOBILE HAMBURGER MENU ===== */\n\n/* Desktop controls - hidden on mobile */\n.desktop-controls {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  order: 3;\n}\n\n/* Mobile hamburger button - hidden on desktop */\n.mobile-hamburger-btn {\n  display: none;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  width: 36px;\n  height: 36px;\n  background: var(--bg-secondary, #f9fafb);\n  border: 1px solid var(--border-light, #e5e7eb);\n  border-radius: 8px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  order: 3;\n  flex-shrink: 0;\n}\n\n.mobile-hamburger-btn:hover {\n  background: var(--bg-hover, #ede9fe);\n  border-color: var(--color-primary, #6366f1);\n}\n\n.hamburger-lines {\n  display: flex;\n  flex-direction: column;\n  gap: 3px;\n  width: 18px;\n  height: 14px;\n  position: relative;\n}\n\n.hamburger-lines span {\n  display: block;\n  width: 100%;\n  height: 2px;\n  background: var(--text-primary, #1f2937);\n  border-radius: 1px;\n  transition: all 0.3s cubic-bezier(0.68, -0.55, 0.265, 1.55);\n  transform-origin: center;\n}\n\n/* Animated hamburger to X */\n.hamburger-lines.open span:nth-child(1) {\n  transform: rotate(45deg) translate(5px, 5px);\n}\n\n.hamburger-lines.open span:nth-child(2) {\n  opacity: 0;\n  transform: scaleX(0);\n}\n\n.hamburger-lines.open span:nth-child(3) {\n  transform: rotate(-45deg) translate(5px, -5px);\n}\n\n/* Mobile slide-out menu */\n.mobile-slide-menu {\n  position: fixed;\n  top: 0;\n  right: -350px; /* Start off-screen */\n  width: 320px;\n  height: 100vh;\n  background: var(--bg-card, linear-gradient(135deg, #ffffff 0%, #f8fafc 100%));\n  border-left: 1px solid var(--border-light, #e5e7eb);\n  box-shadow: -4px 0 20px rgba(0, 0, 0, 0.1);\n  transition: right 0.4s cubic-bezier(0.25, 0.8, 0.25, 1);\n  z-index: 10000;\n  overflow-y: auto;\n  \n  /* iOS safe area support */\n  padding-top: max(20px, env(safe-area-inset-top));\n  padding-bottom: max(20px, env(safe-area-inset-bottom));\n  padding-left: 20px;\n  padding-right: max(20px, env(safe-area-inset-right));\n}\n\n.mobile-slide-menu.open {\n  right: 0; /* Slide in */\n}\n\n.mobile-menu-backdrop {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100vw;\n  height: 100vh;\n  background: rgba(0, 0, 0, 0.5);\n  z-index: 9999;\n  opacity: 0;\n  animation: fadeIn 0.3s ease forwards;\n}\n\n@keyframes fadeIn {\n  to { opacity: 1; }\n}\n\n.mobile-menu-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 24px;\n  padding-bottom: 16px;\n  border-bottom: 1px solid var(--border-light, #e5e7eb);\n}\n\n.mobile-menu-header h3 {\n  font-size: 18px;\n  font-weight: var(--font-bold);\n  color: var(--text-primary, #1f2937);\n  margin: 0;\n}\n\n.close-menu-btn {\n  width: 32px;\n  height: 32px;\n  border: none;\n  background: var(--bg-secondary, #f3f4f6);\n  color: var(--text-secondary, #6b7280);\n  border-radius: 50%;\n  cursor: pointer;\n  font-size: 16px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  transition: all 0.2s ease;\n}\n\n.close-menu-btn:hover {\n  background: var(--error-color, #ef4444);\n  color: white;\n  transform: scale(1.1);\n}\n\n.mobile-menu-section {\n  margin-bottom: 32px;\n}\n\n.mobile-menu-section h4 {\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  color: var(--text-primary, #1f2937);\n  margin: 0 0 16px 0;\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n/* Mobile theme grid */\n.mobile-theme-grid {\n  display: grid;\n  grid-template-columns: 1fr 1fr 1fr;\n  gap: 12px;\n}\n\n.mobile-theme-btn {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 8px;\n  padding: 16px 8px;\n  background: var(--bg-secondary, #f9fafb);\n  border: 2px solid var(--border-light, #e5e7eb);\n  border-radius: 12px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  min-height: 80px;\n}\n\n.mobile-theme-btn:hover {\n  border-color: var(--color-primary, #6366f1);\n  transform: translateY(-2px);\n  box-shadow: 0 4px 12px rgba(99, 102, 241, 0.15);\n}\n\n.mobile-theme-btn.active {\n  background: linear-gradient(135deg, var(--color-primary, #6366f1), #5a67d8);\n  border-color: var(--color-primary, #6366f1);\n  color: white;\n  transform: scale(1.05);\n}\n\n.mobile-theme-btn .theme-icon {\n  font-size: 24px;\n}\n\n.mobile-theme-btn .theme-label {\n  font-size: 12px;\n  font-weight: var(--font-medium);\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n/* Mobile color grid */\n.mobile-color-grid {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 8px;\n}\n\n.mobile-color-btn {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 12px 16px;\n  background: var(--bg-secondary, #f9fafb);\n  border: 2px solid var(--border-light, #e5e7eb);\n  border-radius: 10px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  text-align: left;\n}\n\n.mobile-color-btn:hover {\n  border-color: var(--color-primary, #6366f1);\n  transform: translateY(-1px);\n  box-shadow: 0 2px 8px rgba(99, 102, 241, 0.1);\n}\n\n.mobile-color-btn.active {\n  background: linear-gradient(135deg, var(--color-primary, #6366f1), #5a67d8);\n  border-color: var(--color-primary, #6366f1);\n  color: white;\n}\n\n.mobile-color-btn .color-icon {\n  font-size: 20px;\n  flex-shrink: 0;\n}\n\n.mobile-color-btn .color-label {\n  font-size: 14px;\n  font-weight: var(--font-medium);\n}\n\n/* Mobile logout section */\n.mobile-logout-section {\n  border-top: 1px solid var(--border-light, #e5e7eb);\n  padding-top: 24px;\n}\n\n.mobile-logout-btn {\n  width: 100%;\n  padding: 16px;\n  background: var(--bg-secondary, #f1f5f9);\n  color: var(--text-secondary, #64748b);\n  border: 1px solid var(--border-light, #e2e8f0);\n  border-radius: 12px;\n  font-size: 16px;\n  font-weight: var(--font-medium, 500);\n  cursor: pointer;\n  transition: all 0.3s ease;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 8px;\n}\n\n.mobile-logout-btn:hover {\n  background: var(--bg-hover, #e2e8f0);\n  color: var(--text-primary, #1e293b);\n  border-color: var(--border-medium, #cbd5e1);\n  transform: translateY(-2px);\n  box-shadow: 0 4px 12px rgba(100, 116, 139, 0.1);\n}\n\n/* ===== DROPDOWN COLOR SELECTOR ===== */\n\n.color-dropdown-container {\n  position: relative;\n  display: none; /* Hidden by default, shown on mobile */\n  z-index: 1000;\n}\n\n.color-dropdown-trigger {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  padding: 8px 12px;\n  background: var(--bg-secondary, #f9fafb);\n  border: 1px solid var(--border-light, #e5e7eb);\n  border-radius: 8px;\n  cursor: pointer;\n  transition: all 0.2s ease;\n  min-width: 120px;\n  justify-content: space-between;\n  font-size: 14px;\n  white-space: nowrap;\n}\n\n.color-dropdown-trigger:hover {\n  background: var(--bg-hover, #ede9fe);\n  border-color: var(--color-primary, #6366f1);\n}\n\n.color-dropdown-trigger .color-icon {\n  font-size: 16px;\n  flex-shrink: 0;\n}\n\n.color-dropdown-trigger .color-name {\n  flex: 1 1;\n  text-align: left;\n  font-weight: var(--font-medium);\n}\n\n.color-dropdown-trigger .dropdown-arrow {\n  font-size: 12px;\n  color: var(--text-secondary);\n  transition: transform 0.2s ease;\n}\n\n.color-dropdown-menu {\n  position: absolute;\n  top: 100%;\n  right: 0;\n  min-width: 200px;\n  background: var(--bg-card, #ffffff);\n  border: 1px solid var(--border-light, #e5e7eb);\n  border-radius: 8px;\n  box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);\n  z-index: 10000;\n  max-height: 300px;\n  overflow-y: auto;\n  margin-top: 4px;\n  backdrop-filter: blur(10px);\n  -webkit-backdrop-filter: blur(10px);\n}\n\n.color-dropdown-item {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 12px 16px;\n  width: 100%;\n  background: none;\n  border: none;\n  cursor: pointer;\n  transition: all 0.2s ease;\n  font-size: 14px;\n  text-align: left;\n  border-bottom: 1px solid var(--border-light, #f3f4f6);\n}\n\n.color-dropdown-item:last-child {\n  border-bottom: none;\n}\n\n.color-dropdown-item:hover {\n  background: var(--bg-hover, #f8fafc);\n}\n\n.color-dropdown-item.active {\n  background: var(--color-primary, #6366f1);\n  color: white;\n}\n\n.color-dropdown-item .color-icon {\n  font-size: 16px;\n  flex-shrink: 0;\n}\n\n.color-dropdown-item .color-name {\n  flex: 1 1;\n  font-weight: var(--font-medium);\n}\n\n.color-dropdown-item .checkmark {\n  font-size: 14px;\n  font-weight: bold;\n  color: var(--text-accent, white);\n}\n\n/* Desktop fallback */\n.desktop-only {\n  display: flex;\n}\n\n.theme-btn,\n.color-btn {\n  padding: 4px 6px;\n  border: 1px solid var(--border-light, #e5e7eb);\n  background: var(--bg-secondary, #f9fafb);\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 14px;\n  transition: all 0.2s ease;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-width: 28px;\n  height: 28px;\n  position: relative;\n}\n\n.theme-btn:hover,\n.color-btn:hover {\n  background: var(--bg-hover, #ede9fe);\n  border-color: var(--color-primary, #6366f1);\n  transform: translateY(-1px);\n  box-shadow: 0 2px 4px rgba(99, 102, 241, 0.2);\n}\n\n.theme-btn.active,\n.color-btn.active {\n  background: var(--color-primary, #6366f1);\n  border-color: var(--color-primary, #6366f1);\n  color: white;\n  box-shadow: 0 2px 8px rgba(99, 102, 241, 0.3);\n  transform: translateY(-1px);\n}\n\n.theme-btn.active::after,\n.color-btn.active::after {\n  content: '';\n  position: absolute;\n  bottom: -3px;\n  left: 50%;\n  transform: translateX(-50%);\n  width: 0;\n  height: 0;\n  border-left: 3px solid transparent;\n  border-right: 3px solid transparent;\n  border-bottom: 3px solid var(--color-primary, #6366f1);\n}\n\n/* Color-specific active states */\n.color-btn[data-color=\"purple\"].active {\n  background: #8b5cf6;\n  border-color: #8b5cf6;\n}\n\n.color-btn[data-color=\"blue\"].active {\n  background: #3b82f6;\n  border-color: #3b82f6;\n}\n\n.color-btn[data-color=\"green\"].active {\n  background: #10b981;\n  border-color: #10b981;\n}\n\n.color-btn[data-color=\"red\"].active {\n  background: #ef4444;\n  border-color: #ef4444;\n}\n\n.color-btn[data-color=\"orange\"].active {\n  background: #f59e0b;\n  border-color: #f59e0b;\n}\n\n.color-btn[data-color=\"purple\"].active::after { border-bottom-color: #8b5cf6; }\n.color-btn[data-color=\"blue\"].active::after { border-bottom-color: #3b82f6; }\n.color-btn[data-color=\"green\"].active::after { border-bottom-color: #10b981; }\n.color-btn[data-color=\"red\"].active::after { border-bottom-color: #ef4444; }\n.color-btn[data-color=\"orange\"].active::after { border-bottom-color: #f59e0b; }\n\n/* Logout Button */\n.logout-btn {\n  padding: 8px 16px;\n  background: var(--bg-secondary, #f1f5f9);\n  color: var(--text-secondary, #64748b);\n  border: 1px solid var(--border-light, #e2e8f0);\n  border-radius: 8px;\n  font-size: 14px;\n  font-weight: var(--font-medium, 500);\n  cursor: pointer;\n  transition: all 0.2s ease;\n  display: flex;\n  align-items: center;\n  gap: 4px;\n  white-space: nowrap;\n}\n\n.logout-btn:hover {\n  background: var(--bg-hover, #e2e8f0);\n  color: var(--text-primary, #1e293b);\n  border-color: var(--border-medium, #cbd5e1);\n  transform: translateY(-1px);\n  box-shadow: 0 2px 4px rgba(100, 116, 139, 0.1);\n}\n\n/* Dark Theme Adjustments */\n[data-theme=\"dark\"] .app-header {\n  background: var(--bg-card, linear-gradient(135deg, #1f2937 0%, #111827 100%));\n  border-bottom-color: var(--border-light, #374151);\n}\n\n[data-theme=\"dark\"] .user-info,\n[data-theme=\"dark\"] .theme-controls {\n  background: var(--bg-card, #374151);\n  border-color: var(--border-light, #4b5563);\n}\n\n[data-theme=\"dark\"] .theme-btn,\n[data-theme=\"dark\"] .color-btn {\n  background: var(--bg-secondary, #4b5563);\n  border-color: var(--border-light, #6b7280);\n  color: var(--text-primary, #f9fafb);\n}\n\n[data-theme=\"dark\"] .theme-btn:hover,\n[data-theme=\"dark\"] .color-btn:hover {\n  background: var(--bg-hover, #312e81);\n  border-color: var(--color-primary, #818cf8);\n}\n\n[data-theme=\"dark\"] .app-title {\n  color: var(--color-primary, #818cf8);\n}\n\n/* Responsive Design */\n@media (max-width: 1024px) {\n  .header-content {\n    padding: 10px 16px;\n    gap: 16px;\n  }\n  \n  .app-title {\n    font-size: 16px;\n  }\n  \n  .theme-controls {\n    gap: 12px;\n  }\n  \n  .header-center {\n    max-width: 250px;\n  }\n}\n\n@media (max-width: 768px) {\n  .header-content {\n    padding: 8px 12px;\n    gap: 12px;\n  }\n  \n  .app-title {\n    display: none;\n  }\n  \n  .header-center {\n    display: none;\n  }\n  \n  .theme-controls {\n    gap: 8px;\n    padding: 6px 8px;\n  }\n  \n  .control-label {\n    display: none;\n  }\n  \n  .theme-btn,\n  .color-btn {\n    min-width: 24px;\n    height: 24px;\n    font-size: 12px;\n  }\n  \n  .logout-btn {\n    padding: 6px 10px;\n    font-size: 12px;\n  }\n}\n\n@media (max-width: 480px) {\n  .header-content {\n    padding: 6px 8px;\n    gap: 8px;\n  }\n  \n  .theme-section,\n  .color-section {\n    gap: 4px;\n  }\n  \n  .theme-buttons,\n  .color-buttons {\n    gap: 1px;\n  }\n  \n  .logout-btn span {\n    display: none;\n  }\n}\n\n/* ===== COMPREHENSIVE iOS SAFARI OPTIMIZATIONS ===== */\n\n/* Base iOS optimizations for all iPhone devices */\n@media (max-width: 428px) {\n  .app-header {\n    position: sticky;\n    top: 0;\n    z-index: 1000;\n    backdrop-filter: blur(10px);\n    /* Critical iPhone fixes */\n    -webkit-text-size-adjust: 100%;\n    -webkit-font-smoothing: antialiased;\n    text-rendering: optimizeLegibility;\n  }\n  \n  /* Mobile dropdown color selector */\n  .color-dropdown-container {\n    display: block !important; /* Show dropdown on mobile */\n  }\n  \n  .color-buttons.desktop-only {\n    display: none !important; /* Hide horizontal buttons on mobile */\n  }\n  \n  .header-content {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    flex-wrap: nowrap; /* Prevent wrapping on small screens */\n    min-height: 44px; /* iOS minimum touch target */\n    box-sizing: border-box;\n    -webkit-overflow-scrolling: touch;\n    overflow: visible; /* Prevent content clipping */\n  }\n  \n  /* Prevent text selection and tap highlights */\n  .app-header * {\n    -webkit-user-select: none;\n    user-select: none;\n    -webkit-tap-highlight-color: transparent;\n    -webkit-touch-callout: none;\n  }\n  \n  /* Critical iOS missing features */\n  .header-content {\n    -webkit-overflow-scrolling: touch;\n  }\n  \n  /* Text overflow handling */\n  .app-title,\n  .user-name {\n    white-space: nowrap;\n    text-overflow: ellipsis;\n    overflow: hidden;\n    max-width: 100%;\n  }\n  \n  /* Touch optimizations */\n  .theme-btn,\n  .color-btn,\n  .logout-btn {\n    touch-action: manipulation;\n    -webkit-tap-highlight-color: transparent;\n    cursor: pointer;\n    min-height: 44px; /* iOS minimum touch target */\n    min-width: 44px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n  \n  .theme-btn:active,\n  .color-btn:active {\n    transform: scale(0.95);\n    transition: transform 0.1s ease;\n    will-change: transform;\n  }\n  \n  .logout-btn:active {\n    transform: scale(0.98);\n    transition: transform 0.1s ease;\n    will-change: transform;\n  }\n  \n  /* GPU acceleration for performance */\n  .theme-btn,\n  .color-btn,\n  .logout-btn {\n    transform: translateZ(0);\n    backface-visibility: hidden;\n  }\n}\n\n/* iPhone SE (375px) specific optimizations */\n@media (max-width: 375px) {\n  .header-content {\n    padding: 8px 12px;\n    gap: 8px;\n    min-height: 52px;\n  }\n  \n  .header-left {\n    flex-shrink: 0;\n    min-width: 0;\n  }\n  \n  .app-logo {\n    gap: 6px;\n  }\n  \n  .logo-icon {\n    font-size: 16px;\n    flex-shrink: 0;\n  }\n  \n  .app-title {\n    font-size: 14px !important;\n    max-width: 80px; /* Limit title width */\n    display: none; /* Hide on very small screens */\n  }\n  \n  .header-center {\n    flex: 1 1;\n    min-width: 0;\n    max-width: 120px;\n  }\n  \n  .user-info {\n    padding: 6px 8px;\n    border-radius: 16px;\n    min-width: 0;\n  }\n  \n  .user-name {\n    font-size: 11px !important;\n    font-weight: var(--font-medium);\n    max-width: 80px;\n  }\n  \n  .user-role {\n    font-size: 9px;\n    display: none; /* Hide role on small screens */\n  }\n  \n  .header-right {\n    flex-shrink: 0;\n  }\n  \n  .theme-controls {\n    padding: 4px 6px;\n    gap: 4px;\n    border-radius: 8px;\n  }\n  \n  .control-label {\n    display: none; /* Hide labels */\n  }\n  \n  .theme-section,\n  .color-section {\n    gap: 2px;\n  }\n  \n  /* Compact color dropdown for iPhone SE */\n  .color-dropdown-trigger {\n    min-width: 80px;\n    padding: 6px 8px;\n    font-size: 12px;\n    -webkit-tap-highlight-color: transparent;\n  }\n  \n  .color-dropdown-trigger .color-name {\n    display: none; /* Hide text, show only icon */\n  }\n  \n  .color-dropdown-item {\n    padding: 10px 12px;\n    font-size: 13px;\n    -webkit-tap-highlight-color: transparent;\n  }\n  \n  .theme-btn,\n  .color-btn {\n    min-width: 32px;\n    height: 32px;\n    font-size: 12px;\n    border-radius: 6px;\n    padding: 4px;\n  }\n  \n  .logout-btn {\n    padding: 8px 12px;\n    font-size: 12px;\n    min-height: 44px;\n    border-radius: 8px;\n  }\n  \n  .logout-btn span {\n    display: none; /* Show only icon */\n  }\n}\n\n/* iPhone 12/13/14 (390px) optimizations */\n@media (min-width: 376px) and (max-width: 390px) {\n  .header-content {\n    padding: 10px 16px;\n    gap: 12px;\n    min-height: 54px;\n  }\n  \n  .app-title {\n    font-size: 15px !important;\n    max-width: 100px;\n  }\n  \n  .header-center {\n    max-width: 140px;\n  }\n  \n  .user-info {\n    padding: 6px 12px;\n  }\n  \n  .user-name {\n    font-size: 12px !important;\n    max-width: 100px;\n  }\n  \n  .user-role {\n    font-size: 10px;\n  }\n  \n  .theme-controls {\n    padding: 6px 8px;\n    gap: 6px;\n  }\n  \n  /* Color dropdown for iPhone 12/13/14 */\n  .color-dropdown-trigger {\n    min-width: 100px;\n    padding: 7px 10px;\n    font-size: 13px;\n  }\n  \n  .color-dropdown-trigger .color-name {\n    font-size: 12px;\n  }\n  \n  .color-dropdown-item {\n    padding: 11px 14px;\n    font-size: 13px;\n  }\n  \n  .theme-btn,\n  .color-btn {\n    min-width: 36px;\n    height: 36px;\n    font-size: 13px;\n  }\n  \n  .logout-btn {\n    padding: 8px 14px;\n    font-size: 13px;\n    min-height: 44px;\n  }\n}\n\n/* iPhone 11 (414px) optimizations - NEW */\n@media (min-width: 391px) and (m