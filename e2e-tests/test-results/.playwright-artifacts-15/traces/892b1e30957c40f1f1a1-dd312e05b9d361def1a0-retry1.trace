xt-link-hover: #fb923c;\n  --text-accent: #ffffff;         /* Pure white for buttons and high-contrast elements */\n  --text-header: #ffffff;         /* White for headers */\n  \n  /* Border colors */\n  --border-color: rgba(255, 255, 255, 0.1);\n  --border-light: rgba(255, 255, 255, 0.06);\n  --border-focus: #ffa726;\n  \n  --shadow-primary: 0 4px 6px rgba(0, 0, 0, 0.4);\n  --shadow-secondary: 0 8px 15px rgba(0, 0, 0, 0.6);\n  --shadow-hover: 0 12px 25px rgba(255, 149, 0, 0.25);\n  --shadow-focus: 0 0 0 3px rgba(255, 149, 0, 0.3);\n  \n  /* Button colors - ensuring proper contrast on dark */\n  --btn-primary-bg: #ea580c;      /* 4.67:1 ratio with white text */\n  --btn-primary-hover: #c2410c;\n  --btn-secondary-bg: #fb923c;    /* Good contrast maintained */\n  --btn-secondary-hover: #ea580c;  /* Darker orange for hover */\n  --btn-success-bg: #059669;      /* 4.91:1 ratio with white text */\n  --btn-error-bg: #dc2626;        /* 4.89:1 ratio with white text */\n  --btn-warning-bg: #d97706;      /* 4.26:1 ratio with white text */\n  --btn-warning-text: #ffffff;\n  \n  /* Form elements for dark theme */\n  --input-bg: #374151;\n  --input-border: #4b5563;\n  --input-border-focus: #ffa726;\n  --input-text: #f9fafb;\n  --input-placeholder: #9ca3af;\n}\n\n/* ===== ACCESSIBILITY UTILITIES ===== */\n\n/* High contrast mode support */\n@media (prefers-contrast: high) {\n  :root {\n    --text-primary: #000000 !important;\n    --text-secondary: #1a1a1a !important;\n    --border-color: #000000 !important;\n    --btn-primary-bg: #000080 !important;\n    --btn-success-bg: #006400 !important;\n    --btn-error-bg: #8b0000 !important;\n  }\n  \n  :root[data-theme=\"dark\"] {\n    --text-primary: #ffffff !important;\n    --text-secondary: #e6e6e6 !important;\n    --border-color: #ffffff !important;\n    --bg-card: #000000 !important;\n  }\n}\n\n/* Reduced motion support */\n@media (prefers-reduced-motion: reduce) {\n  * {\n    animation-duration: 0.01ms !important;\n    animation-iteration-count: 1 !important;\n    transition-duration: 0.01ms !important;\n  }\n}\n\n/* Focus indicators for keyboard navigation */\n.accessible-focus:focus,\nbutton:focus,\ninput:focus,\nselect:focus,\ntextarea:focus,\na:focus {\n  outline: 2px solid var(--border-focus);\n  outline-offset: 2px;\n  box-shadow: var(--shadow-focus);\n}\n\n/* Screen reader only content */\n.sr-only {\n  position: absolute;\n  width: 1px;\n  height: 1px;\n  padding: 0;\n  margin: -1px;\n  overflow: hidden;\n  clip: rect(0, 0, 0, 0);\n  white-space: nowrap;\n  border: 0;\n}\n\n/* Skip to main content link */\n.skip-link {\n  position: absolute;\n  top: -40px;\n  left: 6px;\n  background: var(--btn-primary-bg);\n  color: white;\n  padding: 8px;\n  text-decoration: none;\n  z-index: 1000;\n  border-radius: 4px;\n}\n\n.skip-link:focus {\n  top: 6px;\n}\n\n/* ===== COMPONENT-SPECIFIC ACCESSIBILITY IMPROVEMENTS ===== */\n\n/* Ensure sufficient contrast for all interactive elements */\n.btn-primary {\n  background-color: var(--btn-primary-bg);\n  color: white;\n  border: 2px solid var(--btn-primary-bg);\n}\n\n.btn-primary:hover {\n  background-color: var(--btn-primary-hover);\n  border-color: var(--btn-primary-hover);\n}\n\n.btn-secondary {\n  background-color: var(--btn-secondary-bg);\n  color: white;\n  border: 2px solid var(--btn-secondary-bg);\n}\n\n.btn-success {\n  background-color: var(--btn-success-bg);\n  color: white;\n  border: 2px solid var(--btn-success-bg);\n}\n\n.btn-error {\n  background-color: var(--btn-error-bg);\n  color: white;\n  border: 2px solid var(--btn-error-bg);\n}\n\n.btn-warning {\n  background-color: var(--btn-warning-bg);\n  color: var(--btn-warning-text);\n  border: 2px solid var(--btn-warning-bg);\n}\n\n/* Link styles with proper contrast */\na {\n  color: var(--text-link);\n  text-decoration: underline;\n}\n\na:hover {\n  color: var(--text-link-hover);\n}\n\n/* Form inputs with accessibility improvements */\ninput, textarea, select {\n  background-color: var(--