d letter spacing */\n  position: relative;\n  overflow: hidden;\n  height: 44px; /* Fixed height for consistency */\n}\n\n.action-btn::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: -100%;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(90deg, transparent, rgba(255,255,255,0.2), transparent);\n  transition: left 0.5s;\n}\n\n.action-btn:hover::before {\n  left: 100%;\n}\n\n.action-btn:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 8px 25px rgba(0, 0, 0, 0.15);\n}\n\n/* Primary buttons inherit from universal system - no manual colors needed */\n\n/* Secondary buttons now use universal system - no manual colors needed */\n\n/* Hover states handled by universal system */\n\n.action-btn:disabled,\n.action-btn.disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n  transform: none !important;\n  background: var(--color-disabled) !important;\n  color: var(--text-disabled) !important;\n  border-color: var(--border-disabled) !important;\n}\n\n/* Tertiary button style for waitlist */\n.action-btn.tertiary {\n  background: transparent;\n  color: var(--text-muted);\n  border-color: var(--border-light);\n  border-style: solid;\n}\n\n.action-btn.tertiary:hover:not(:disabled) {\n  background: var(--bg-hover);\n  color: var(--text-primary);\n  border-color: var(--border-focus);\n}\n\n/* All theme-specific overrides removed - universal system handles everything automatically */\n\n/* Auto mode handled by unified color system automatically */\n\n/* Responsive design */\n@media (max-width: 768px) {\n  .project-card {\n    margin-bottom: 20px;\n  }\n  \n  .project-header {\n    flex-direction: column;\n    gap: 16px;\n    padding: 20px 20px 0 20px;\n  }\n  \n  .project-title {\n    font-size: var(--text-xl);\n  }\n  \n  .project-meta {\n    gap: 8px;\n    justify-content: center;\n  }\n  \n  .meta-badge {\n    padding: 6px 10px;\n    font-size: var(--text-xs);\n  }\n  \n  .project-description {\n    padding: 0 20px;\n    margin-bottom: 20px