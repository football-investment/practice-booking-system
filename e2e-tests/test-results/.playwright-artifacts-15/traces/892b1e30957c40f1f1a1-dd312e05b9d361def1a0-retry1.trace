lor: var(--text-accent);\n}\n\n.page-header h1 {\n  font-size: 28px;\n  font-weight: var(--font-bold);\n  color: var(--text-header);\n  margin: 8px 0;\n}\n\n.back-link {\n  color: var(--text-accent);\n  text-decoration: none;\n  font-size: 14px;\n  transition: color 0.2s ease;\n  font-weight: var(--font-medium);\n}\n\n.back-link:hover {\n  color: var(--text-accent);\n  text-shadow: 0 0 8px rgba(255, 255, 255, 0.5);\n}\n\n.logout-btn {\n  padding: 12px 20px;\n  background: var(--bg-glass);\n  color: var(--text-accent);\n  border: 1px solid var(--border-color);\n  border-radius: 12px;\n  font-size: 14px;\n  cursor: pointer;\n  transition: all 0.2s ease;\n}\n\n.logout-btn:hover {\n  background: rgba(255, 255, 255, 0.2);\n  transform: translateY(-1px);\n}\n\n/* Loading State */\n.page-loading {\n  background: var(--bg-card);\n  border-radius: 20px;\n  box-shadow: var(--shadow-primary);\n  padding: 60px;\n  text-align: center;\n  color: var(--text-secondary);\n  font-size: 18px;\n  margin: 20px auto;\n  max-width: 500px;\n  backdrop-filter: blur(10px);\n  border: 1px solid var(--border-color);\n}\n\n/* Notification Banners */\n.error-banner {\n  background: #fed7d7;\n  color: #e53e3e;\n  padding: 12px 16px;\n  border-radius: 12px;\n  font-size: 14px;\n  text-align: center;\n  margin: 20px 0;\n  border: 1px solid #feb2b2;\n}\n\n.success-banner {\n  background: #c6f6d5;\n  color: #2f855a;\n  padding: 12px 16px;\n  border-radius: 12px;\n  font-size: 14px;\n  text-align: center;\n  margin: 20px 0;\n  border: 1px solid #9ae6b4;\n}\n\n/* Profile Content */\n.profile-content {\n  display: flex;\n  flex-direction: column;\n  gap: 25px;\n  max-width: 1000px;\n  margin: 0 auto;\n}\n\n/* Profile Row for side-by-side sections */\n.profile-row {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 25px;\n}\n\n/* Half-width sections */\n.half-width {\n  width: 100%;\n}\n\n/* Main profile section styling */\n.main-profile {\n  /* Existing styling preserved */\n}\n\n/* Profile Section - matches login card style */\n.profile-section {\n  background: var(--bg-card);\n  border-radius: 20px;\n  box-shadow: 0 20px 40px var(--shadow-primary);\n  padding: 40px;\n  transition: all 0.2s ease;\n  backdrop-filter: blur(10px);\n  border: 1px solid var(--border-color);\n}\n\n.profile-section:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 25px 50px rgba(0, 0, 0, 0.15);\n}\n\n.profile-section h2 {\n  color: var(--text-primary);\n  font-size: 24px;\n  font-weight: var(--font-bold);\n  margin-bottom: 25px;\n  text-align: center;\n  border-bottom: 2px solid var(--border-color);\n  padding-bottom: 15px;\n}\n\n/* Forms */\n.profile-form,\n.password-form {\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.form-row {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 20px;\n}\n\n.form-group {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.form-group label {\n  color: var(--text-primary);\n  font-size: 14px;\n  font-weight: var(--font-semibold);\n  margin-bottom: 5px;\n}\n\n/* Form Inputs - matches login input style */\n.form-input,\n.form-select {\n  width: 100%;\n  padding: 16px 20px;\n  border: 2px solid var(--border-color);\n  border-radius: 12px;\n  font-size: 16px;\n  transition: all 0.2s ease;\n  background: var(--bg-secondary);\n  color: var(--text-primary);\n}\n\n.form-input:focus,\n.form-select:focus {\n  outline: none;\n  border-color: var(--text-primary);\n  background: var(--bg-card);\n  box-shadow: var(--shadow-secondary);\n}\n\n.form-select {\n  cursor: pointer;\n}\n\n.form-input::placeholder {\n  color: var(--text-secondary);\n}\n\n/* Buttons - consistent with login button style */\n.save-btn,\n.change-password-btn {\n  width: 100%;\n  padding: 16px;\n  background: linear-gradient(135deg, #4299e1, #3182ce);\n  color: white;\n  border: none;\n  border-radius: 12px;\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  cursor: pointer;\n  transition: all 0.2s ease;\n  margin-top: 20px;\n}\n\n.save-btn:hover:not(:disabled),\n.change-password-btn:hover:not(:disabled) {\n  transform: translateY(-2px);\n  box-shadow: 0 8px 25px rgba(66, 153, 225, 0.3);\n}\n\n.save-btn:disabled,\n.change-password-btn:disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n}\n\n.change-password-btn {\n  background: linear-gradient(135deg, #38a169, #2f855a);\n}\n\n.change-password-btn:hover:not(:disabled) {\n  box-shadow: 0 8px 25px rgba(56, 161, 105, 0.3);\n}\n\n/* Account Statistics */\n.stats-overview {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n  gap: 20px;\n  margin-top: 20px;\n}\n\n.stats-overview.compact {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  margin-top: 15px;\n}\n\n.stats-overview.compact .stat-item {\n  padding: 15px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.stats-overview.compact .stat-label {\n  font-size: 13px;\n  margin: 0;\n}\n\n.stats-overview.compact .stat-value {\n  font-size: 18px;\n  font-weight: var(--font-bold);\n}\n\n.stat-item {\n  background: var(--bg-secondary);\n  border: 2px solid var(--border-color);\n  border-radius: 12px;\n  padding: 25px;\n  text-align: center;\n  transition: all 0.2s ease;\n}\n\n.stat-item:hover {\n  border-color: var(--text-primary);\n  background: var(--bg-card);\n  transform: translateY(-2px);\n  box-shadow: var(--shadow-hover);\n}\n\n.stat-label {\n  display: block;\n  color: var(--text-secondary);\n  font-size: 14px;\n  font-weight: var(--font-medium);\n  margin-bottom: 8px;\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.stat-value {\n  display: block;\n  color: var(--text-primary);\n  font-size: 28px;\n  font-weight: var(--font-bold);\n}\n\n/* Special styling for attendance rate */\n.stat-item:last-child .stat-value {\n  color: #38a169;\n}\n\n/* Enhanced hover effects */\n.profile-section:hover .stat-item {\n  transform: translateY(-1px);\n}\n\n/* Form validation states */\n.form-input:invalid:not(:focus):not(:placeholder-shown) {\n  border-color: #e53e3e;\n  background: var(--bg-secondary);\n}\n\n.form-input:valid:not(:focus):not(:placeholder-shown) {\n  border-color: #38a169;\n  background: var(--bg-secondary);\n}\n\n/* Loading states for buttons */\n.save-btn:disabled::before,\n.change-password-btn:disabled::before {\n  content: \"‚è≥ \";\n  animation: spin 1s linear infinite;\n}\n\n@keyframes spin {\n  0% { transform: rotate(0deg); }\n  100% { transform: rotate(360deg); }\n}\n\n/* Form Help Text */\n.form-help {\n  color: var(--text-secondary);\n  font-size: 12px;\n  margin-top: 4px;\n  line-height: 1.3;\n  font-style: italic;\n}\n\n/* Responsive Design */\n@media (max-width: 768px) {\n  .student-profile {\n    padding: 15px;\n  }\n  \n  .page-header {\n    flex-direction: column;\n    gap: 15px;\n    text-align: center;\n  }\n  \n  .profile-section {\n    padding: 25px;\n  }\n  \n  .profile-row {\n    grid-template-columns: 1fr;\n    gap: 20px;\n  }\n  \n  .form-row {\n    grid-template-columns: 1fr;\n    gap: 15px;\n  }\n  \n  .stats-overview {\n    grid-template-columns: 1fr;\n    gap: 15px;\n  }\n  \n  .profile-section h2 {\n    font-size: 20px;\n  }\n}\n\n@media (max-width: 480px) {\n  .profile-section {\n    padding: 20px;\n  }\n  \n  .form-input,\n  .form-select {\n    padding: 14px 16px;\n    font-size: 14px;\n  }\n  \n  .save-btn,\n  .change-password-btn {\n    padding: 14px;\n    font-size: 14px;\n  }\n  \n  .stat-item {\n    padding: 20px;\n  }\n  \n  .stat-value {\n    font-size: 24px;\n  }\n}\n\n/* Focus management for accessibility */\n.form-group:focus-within label {\n  color: #4299e1;\n}\n\n/* Enhanced visual feedback */\n.profile-section {\n  position: relative;\n  overflow: hidden;\n}\n\n.profile-section::before {\n  content: '';\n  position: absolute;\n  top: -2px;\n  left: -2px;\n  right: -2px;\n  height: 4px;\n  background: linear-gradient(135deg, #4299e1, #3182ce);\n  opacity: 0;\n  transition: opacity 0.2s ease;\n}\n\n.profile-section:hover::before {\n  opacity: 1;\n}\n\n/* Password strength indicator would go here if needed */\n.password-form .form-group:nth-child(2) {\n  position: relative;\n}\n\n/* Success states */\n.form-input.success {\n  border-color: #38a169;\n  background: var(--bg-secondary);\n}\n\n.form-input.error {\n  border-color: #e53e3e;\n  background: var(--bg-secondary);\n}\n\n/* Theme Preferences Styles */\n.theme-preferences {\n  display: flex;\n  flex-direction: column;\n  gap: 30px;\n}\n\n.preference-group {\n  display: flex;\n  flex-direction: column;\n  gap: 15px;\n}\n\n.preference-group h3 {\n  color: var(--text-primary);\n  font-size: 18px;\n  font-weight: var(--font-semibold);\n  margin: 0;\n}\n\n.preference-description {\n  color: var(--text-secondary);\n  font-size: 14px;\n  margin: 0;\n}\n\n/* Color Scheme Grid */\n.color-scheme-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));\n  gap: 15px;\n  margin-top: 10px;\n}\n\n.color-scheme-card {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 8px;\n  padding: 20px 15px;\n  background: var(--bg-secondary);\n  border: 2px solid var(--border-color);\n  border-radius: 12px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  text-decoration: none;\n  color: var(--text-primary);\n  font-size: 14px;\n  font-weight: var(--font-medium);\n}\n\n.color-scheme-card:hover {\n  transform: translateY(-3px);\n  border-color: var(--text-primary);\n  box-shadow: var(--shadow-hover);\n}\n\n.color-scheme-card.active {\n  border-color: var(--text-primary);\n  background: var(--bg-glass);\n  transform: translateY(-2px);\n  box-shadow: var(--shadow-secondary);\n}\n\n.color-preview {\n  font-size: 24px;\n  width: 40px;\n  height: 40px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 50%;\n  background: var(--bg-glass);\n}\n\n.color-preview.purple { filter: hue-rotate(0deg); }\n.color-preview.blue { filter: hue-rotate(240deg); }\n.color-preview.green { filter: hue-rotate(120deg); }\n.color-preview.red { filter: hue-rotate(0deg) saturate(1.5); }\n.color-preview.orange { filter: hue-rotate(30deg); }\n\n/* Theme Mode Buttons */\n.theme-mode-buttons {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  margin-top: 10px;\n}\n\n.theme-mode-btn {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n  padding: 18px 20px;\n  background: var(--bg-secondary);\n  border: 2px solid var(--border-color);\n  border-radius: 12px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  text-align: left;\n  color: var(--text-primary);\n}\n\n.theme-mode-btn:hover {\n  transform: translateY(-2px);\n  border-color: var(--text-primary);\n  box-shadow: var(--shadow-hover);\n}\n\n.theme-mode-btn.active {\n  border-color: var(--text-primary);\n  background: var(--bg-glass);\n  transform: translateY(-1px);\n  box-shadow: var(--shadow-secondary);\n}\n\n.theme-icon {\n  font-size: 24px;\n  width: 40px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.theme-info {\n  display: flex;\n  flex-direction: column;\n  gap: 2px;\n}\n\n.theme-name {\n  font-weight: var(--font-semibold);\n  font-size: 16px;\n  color: var(--text-primary);\n}\n\n.theme-desc {\n  font-size: 13px;\n  color: var(--text-secondary);\n}\n\n/* Responsive adjustments for theme preferences */\n@media (max-width: 768px) {\n  .color-scheme-grid {\n    grid-template-columns: repeat(auto-fit, minmax(100px, 1fr));\n    gap: 10px;\n  }\n  \n  .color-scheme-card {\n    padding: 15px 10px;\n    font-size: 13px;\n  }\n  \n  .color-preview {\n    font-size: 20px;\n    width: 35px;\n    height: 35px;\n  }\n  \n  .theme-mode-btn {\n    padding: 15px 16px;\n  }\n  \n  .theme-icon {\n    font-size: 20px;\n    width: 35px;\n  }\n  \n  .theme-name {\n    font-size: 15px;\n  }\n  \n  .theme-desc {\n    font-size: 12px;\n  }\n}\n\n@media (max-width: 480px) {\n  .color-scheme-grid {\n    grid-template-columns: repeat(2, 1fr);\n  }\n  \n  .theme-mode-buttons {\n    gap: 10px;\n  }\n  \n  .theme-preferences {\n    gap: 25px;\n  }\n}\n/*# sourceMappingURL=data:application/json;base64, */"],["STYLE",{},"/* Import theme variables from StudentDashboard.css */\n\n/* FeedbackPage - Modern glassmorphism design with theme support */\n.feedback-page {\n  min-height: 100vh;\n  background: var(--bg-primary);\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', sans-serif;\n  padding: 20px;\n  position: relative;\n  overflow-x: hidden;\n  transition: background 0.3s ease;\n}\n\n/* Animated background pattern */\n.feedback-page::before {\n  content: '';\n  position: fixed;\n  top: -50%;\n  left: -50%;\n  width: 200%;\n  height: 200%;\n  background: radial-gradient(circle, rgba(255,255,255,0.1) 1px, transparent 1px);\n  background-size: 50px 50px;\n  animation: float 20s ease-in-out infinite;\n  pointer-events: none;\n  z-index: 1;\n}\n\n@keyframes float {\n  0%, 100% { transform: translateY(0px) rotate(0deg); }\n  50% { transform: translateY(-20px) rotate(180deg); }\n}\n\n/* Page Header */\n.page-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 30px;\n  padding: 25px 35px;\n  background: rgba(255, 255, 255, 0.15);\n  backdrop-filter: blur(20px);\n  border-radius: 25px;\n  border: 1px solid rgba(255, 255, 255, 0.2);\n  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);\n  position: relative;\n  z-index: 2;\n}\n\n.page-header h1 {\n  font-size: 32px;\n  font-weight: var(--font-extrabold);\n  color: var(--text-header);\n  margin: 0;\n  text-shadow: 0 2px 10px rgba(0, 0, 0, 0.3);\n}\n\n.back-link {\n  color: var(--text-accent);\n  text-decoration: none;\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  padding: 10px 20px;\n  background: rgba(255, 255, 255, 0.2);\n  border-radius: 15px;\n  transition: all 0.3s ease;\n  display: inline-block;\n  margin-bottom: 10px;\n}\n\n.back-link:hover {\n  background: rgba(255, 255, 255, 0.3);\n  transform: translateY(-2px);\n  box-shadow: 0 8px 20px rgba(255, 255, 255, 0.2);\n}\n\n.logout-btn {\n  padding: 15px 25px;\n  background: linear-gradient(135deg, #ff6b6b, #ee5a52);\n  color: var(--text-accent);\n  border: none;\n  border-radius: 15px;\n  font-size: 14px;\n  font-weight: var(--font-semibold);\n  cursor: pointer;\n  transition: all 0.3s ease;\n  box-shadow: 0 8px 20px rgba(255, 107, 107, 0.3);\n}\n\n.logout-btn:hover {\n  transform: translateY(-3px);\n  box-shadow: 0 15px 30px rgba(255, 107, 107, 0.4);\n}\n\n/* Notifications */\n.error-banner {\n  background: linear-gradient(135deg, #fee2e2, #fecaca);\n  color: #dc2626;\n  padding: 18px 25px;\n  border-radius: 18px;\n  font-size: 14px;\n  text-align: center;\n  margin: 20px 0;\n  border: 1px solid #fca5a5;\n  box-shadow: 0 8px 25px rgba(220, 38, 38, 0.2);\n  position: relative;\n  z-index: 2;\n}\n\n.success-banner {\n  background: linear-gradient(135deg, #d1fae5, #a7f3d0);\n  color: #059669;\n  padding: 18px 25px;\n  border-radius: 18px;\n  font-size: 14px;\n  text-align: center;\n  margin: 20px 0;\n  border: 1px solid #6ee7b7;\n  box-shadow: 0 8px 25px rgba(5, 150, 105, 0.2);\n  position: relative;\n  z-index: 2;\n}\n\n/* Main Content */\n.feedback-content {\n  position: relative;\n  z-index: 2;\n  display: flex;\n  flex-direction: column;\n  gap: 30px;\n}\n\n/* Feedback Sections */\n.feedback-section {\n  background: var(--bg-card);\n  backdrop-filter: blur(20px);\n  border-radius: 25px;\n  box-shadow: 0 25px 50px rgba(0, 0, 0, 0.15);\n  padding: 35px;\n  border: 1px solid rgba(255, 255, 255, 0.3);\n}\n\n.feedback-section h2 {\n  font-size: 24px;\n  font-weight: var(--font-bold);\n  color: var(--text-primary);\n  margin-bottom: 25px;\n  text-align: center;\n}\n\n/* Empty State */\n.empty-state {\n  text-align: center;\n  padding: 40px;\n  color: var(--text-secondary);\n}\n\n.empty-state p {\n  font-size: 16px;\n  margin-bottom: 20px;\n}\n\n.cta-button {\n  display: inline-block;\n  padding: 16px 32px;\n  background: linear-gradient(135deg, #4299e1, #3182ce);\n  color: var(--text-accent);\n  text-decoration: none;\n  border-radius: 15px;\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  transition: all 0.3s ease;\n  box-shadow: 0 8px 25px rgba(66, 153, 225, 0.3);\n}\n\n.cta-button:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 15px 35px rgba(66, 153, 225, 0.4);\n}\n\n/* Pending Feedback List */\n.pending-feedback-list {\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.feedback-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 25px;\n  background: rgba(248, 250, 252, 0.8);\n  border-radius: 18px;\n  border: 1px solid rgba(148, 163, 184, 0.2);\n  transition: all 0.3s ease;\n  gap: 20px;\n}\n\n.feedback-item:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);\n  background: var(--bg-card);\n}\n\n.session-info {\n  flex: 1 1;\n}\n\n.session-info h3 {\n  font-size: 18px;\n  font-weight: var(--font-semibold);\n  color: var(--text-primary);\n  margin: 0 0 8px 0;\n}\n\n.session-info p {\n  margin: 4px 0;\n  color: #4a5568;\n  font-size: 14px;\n}\n\n.feedback-btn {\n  padding: 15px 25px;\n  background: linear-gradient(135deg, #10b981, #059669);\n  color: var(--text-accent);\n  border: none;\n  border-radius: 15px;\n  font-size: 14px;\n  font-weight: var(--font-semibold);\n  cursor: pointer;\n  transition: all 0.3s ease;\n  box-shadow: 0 4px 15px rgba(16, 185, 129, 0.3);\n  white-space: nowrap;\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  position: relative;\n  overflow: hidden;\n}\n\n.feedback-btn::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: -100%;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);\n  transition: left 0.5s;\n}\n\n.feedback-btn:hover::before {\n  left: 100%;\n}\n\n.feedback-btn:hover {\n  transform: translateY(-3px);\n  box-shadow: 0 12px 30px rgba(16, 185, 129, 0.4);\n}\n\n.btn-emoji {\n  font-size: 16px;\n  transition: all 0.3s ease;\n}\n\n.feedback-btn:hover .btn-emoji {\n  animation: sparkle 0.8s ease-in-out;\n}\n\n@keyframes sparkle {\n  0%, 100% { transform: scale(1) rotate(0deg); }\n  25% { transform: scale(1.2) rotate(-10deg); }\n  75% { transform: scale(1.2) rotate(10deg); }\n}\n\n.spin {\n  animation: spin 1s linear infinite !important;\n}\n\n@keyframes spin {\n  from { transform: rotate(0deg); }\n  to { transform: rotate(360deg); }\n}\n\n/* Feedback Modal */\n.feedback-modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: rgba(0, 0, 0, 0.5);\n  backdrop-filter: blur(10px);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  z-index: 1000;\n  padding: 20px;\n}\n\n.modal-content {\n  background: var(--bg-card);\n  border-radius: 25px;\n  box-shadow: 0 25px 50px rgba(0, 0, 0, 0.25);\n  max-width: 600px;\n  width: 100%;\n  max-height: 90vh;\n  overflow-y: auto;\n}\n\n.modal-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 25px 30px;\n  border-bottom: 1px solid #e2e8f0;\n}\n\n.modal-header h3 {\n  font-size: 20px;\n  font-weight: var(--font-bold);\n  color: var(--text-primary);\n  margin: 0;\n}\n\n.close-btn {\n  background: none;\n  border: none;\n  font-size: 24px;\n  color: var(--text-secondary);\n  cursor: pointer;\n  padding: 5px;\n  transition: all 0.2s ease;\n}\n\n.close-btn:hover {\n  color: var(--text-primary);\n  transform: scale(1.1);\n}\n\n/* Feedback Form */\n.feedback-form {\n  padding: 30px;\n}\n\n.form-group {\n  margin-bottom: 25px;\n}\n\n.form-group label {\n  display: block;\n  font-size: 18px;\n  font-weight: var(--font-bold);\n  color: var(--text-primary);\n  margin-bottom: 5px;\n}\n\n.rating-description {\n  font-size: 14px;\n  color: var(--text-secondary);\n  margin-bottom: 15px;\n  font-style: italic;\n  opacity: 0.8;\n}\n\n/* Emoji Rating System */\n.emoji-rating {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  margin-bottom: 15px;\n  flex-wrap: wrap;\n}\n\n.emoji-star {\n  background: none;\n  border: none;\n  font-size: 32px;\n  cursor: pointer;\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n  opacity: 0.3;\n  position: relative;\n  padding: 8px;\n  border-radius: 15px;\n  transform: scale(0.9);\n}\n\n.emoji-star:hover {\n  opacity: 0.8;\n  transform: scale(1.1) rotate(5deg);\n  background: rgba(255, 255, 255, 0.1);\n  box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);\n}\n\n.emoji-star.filled {\n  opacity: 1;\n  transform: scale(1.1);\n  background: rgba(66, 153, 225, 0.1);\n  box-shadow: 0 8px 25px rgba(66, 153, 225, 0.2);\n}\n\n.emoji-star.filled:hover {\n  transform: scale(1.2) rotate(-5deg);\n  background: rgba(66, 153, 225, 0.2);\n  box-shadow: 0 12px 35px rgba(66, 153, 225, 0.3);\n}\n\n.emoji-icon {\n  display: inline-block;\n  transition: all 0.3s ease;\n  animation: none;\n}\n\n.emoji-star:hover .emoji-icon {\n  animation: bounce 0.6s ease-in-out;\n}\n\n.emoji-star.filled .emoji-icon {\n  animation: pulse 2s ease-in-out infinite;\n}\n\n@keyframes bounce {\n  0%, 20%, 50%, 80%, 100% { transform: translateY(0); }\n  40% { transform: translateY(-8px); }\n  60% { transform: translateY(-4px); }\n}\n\n@keyframes pulse {\n  0%, 100% { transform: scale(1); }\n  50% { transform: scale(1.05); }\n}\n\n.rating-text {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  margin-left: 15px;\n  font-size: 16px;\n  color: var(--text-secondary);\n  font-weight: var(--font-semibold);\n  background: rgba(248, 250, 252, 0.8);\n  padding: 8px 15px;\n  border-radius: 25px;\n  border: 2px solid rgba(66, 153, 225, 0.2);\n}\n\n.rating-emoji {\n  font-size: 20px;\n  animation: rotate 3s ease-in-out infinite;\n}\n\n@keyframes rotate {\n  0%, 100% { transform: rotate(0deg); }\n  25% { transform: rotate(-5deg); }\n  75% { transform: rotate(5deg); }\n}\n\n.rating-value {\n  font-weight: var(--font-bold);\n  color: #4299e1;\n}\n\n/* Enhanced Recommendation Buttons */\n.recommendation-buttons {\n  display: flex;\n  gap: 20px;\n  margin-top: 15px;\n}\n\n.recommendation-btn {\n  flex: 1 1;\n  padding: 20px;\n  border: 2px solid var(--border-color);\n  background: var(--bg-card);\n  border-radius: 20px;\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  cursor: pointer;\n  transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 8px;\n  position: relative;\n  overflow: hidden;\n}\n\n.recommendation-btn::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: -100%;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.4), transparent);\n  transition: left 0.6s;\n}\n\n.recommendation-btn:hover::before {\n  left: 100%;\n}\n\n.recommendation-btn:hover {\n  border-color: #cbd5e0;\n  transform: translateY(-5px) scale(1.02);\n  box-shadow: 0 15px 35px rgba(0, 0, 0, 0.1);\n}\n\n.rec-emoji {\n  font-size: 32px;\n  transition: all 0.3s ease;\n}\n\n.recommendation-btn:hover .rec-emoji {\n  animation: wiggle 0.8s ease-in-out;\n  transform: scale(1.2);\n}\n\n@keyframes wiggle {\n  0%, 100% { transform: scale(1.2) rotate(0deg); }\n  25% { transform: scale(1.2) rotate(-5deg); }\n  75% { transform: scale(1.2) rotate(5deg); }\n}\n\n.rec-text {\n  font-size: 18px;\n  font-weight: var(--font-bold);\n  transition: all 0.3s ease;\n}\n\n.rec-subtitle {\n  font-size: 12px;\n  opacity: 0.7;\n  font-weight: var(--font-medium);\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.recommendation-btn.active.positive {\n  background: linear-gradient(135deg, #10b981, #059669);\n  color: var(--text-accent);\n  border-color: #10b981;\n  transform: translateY(-5px) scale(1.05);\n  box-shadow: 0 20px 40px rgba(16, 185, 129, 0.3);\n}\n\n.recommendation-btn.active.positive .rec-emoji {\n  animation: celebration 1s ease-in-out infinite;\n}\n\n@keyframes celebration {\n  0%, 100% { transform: scale(1.1); }\n  50% { transform: scale(1.2) rotate(10deg); }\n}\n\n.recommendation-btn.active.negative {\n  background: linear-gradient(135deg, #ef4444, #dc2626);\n  color: var(--text-accent);\n  border-color: #ef4444;\n  transform: translateY(-5px) scale(1.05);\n  box-shadow: 0 20px 40px rgba(239, 68, 68, 0.3);\n}\n\n.recommendation-btn.active.negative .rec-emoji {\n  animation: shake 1s ease-in-out infinite;\n}\n\n@keyframes shake {\n  0%, 100% { transform: scale(1.1); }\n  25% { transform: scale(1.1) translateX(-2px); }\n  75% { transform: scale(1.1) translateX(2px); }\n}\n\n/* Form Textarea */\n.form-textarea {\n  width: 100%;\n  padding: 15px;\n  border: 2px solid var(--border-color);\n  border-radius: 12px;\n  font-size: 14px;\n  font-family: inherit;\n  resize: vertical;\n  min-height: 100px;\n  transition: all 0.2s ease;\n}\n\n.form-textarea:focus {\n  outline: none;\n  border-color: #4299e1;\n  box-shadow: 0 0 0 3px rgba(66, 153, 225, 0.1);\n}\n\n/* Form Actions */\n.form-actions {\n  display: flex;\n  gap: 15px;\n  margin-top: 30px;\n  padding-top: 20px;\n  border-top: 1px solid #e2e8f0;\n}\n\n.cancel-btn {\n  flex: 1 1;\n  padding: 15px 20px;\n  background: #f1f5f9;\n  color: var(--text-secondary);\n  border: none;\n  border-radius: 12px;\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  cursor: pointer;\n  transition: all 0.2s ease;\n}\n\n.cancel-btn:hover {\n  background: #e2e8f0;\n  transform: translateY(-1px);\n}\n\n.submit-btn {\n  flex: 1 1;\n  padding: 18px 25px;\n  background: linear-gradient(135deg, #4299e1, #3182ce);\n  color: var(--text-accent);\n  border: none;\n  border-radius: 15px;\n  font-size: 16px;\n  font-weight: var(--font-bold);\n  cursor: pointer;\n  transition: all 0.3s ease;\n  box-shadow: 0 4px 15px rgba(66, 153, 225, 0.3);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 8px;\n  position: relative;\n  overflow: hidden;\n}\n\n.submit-btn::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: -100%;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);\n  transition: left 0.5s;\n}\n\n.submit-btn:hover:not(:disabled)::before {\n  left: 100%;\n}\n\n.submit-btn:hover:not(:disabled) {\n  transform: translateY(-3px);\n  box-shadow: 0 12px 30px rgba(66, 153, 225, 0.4);\n}\n\n.submit-btn:hover:not(:disabled) .btn-emoji:not(.spin) {\n  animation: rocket 0.6s ease-in-out;\n}\n\n@keyframes rocket {\n  0%, 100% { transform: scale(1) rotate(0deg); }\n  50% { transform: scale(1.2) rotate(15deg) translateY(-2px); }\n}\n\n.submit-btn:disabled {\n  opacity: 0.7;\n  cursor: not-allowed;\n  transform: none;\n}\n\n/* Feedback History */\n.feedback-history {\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.feedback-history-item {\n  padding: 25px;\n  background: rgba(248, 250, 252, 0.8);\n  border-radius: 18px;\n  border: 1px solid rgba(148, 163, 184, 0.2);\n  transition: all 0.3s ease;\n}\n\n.feedback-history-item:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);\n  background: var(--bg-card);\n}\n\n.feedback-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 15px;\n  gap: 15px;\n}\n\n.feedback-header h4 {\n  font-size: 18px;\n  font-weight: var(--font-semibold);\n  color: var(--text-primary);\n  margin: 0;\n  flex: 1 1;\n}\n\n.feedback-actions {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n}\n\n.feedback-rating {\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  color: #059669;\n  display: flex;\n  align-items: center;\n  gap: 5px;\n  background: rgba(16, 185, 129, 0.1);\n  padding: 8px 12px;\n  border-radius: 12px;\n  border: 2px solid rgba(16, 185, 129, 0.2);\n}\n\n.rating-emoji-display {\n  font-size: 18px;\n  animation: glow 2s ease-in-out infinite;\n}\n\n@keyframes glow {\n  0%, 100% { transform: scale(1); }\n  50% { transform: scale(1.1); }\n}\n\n.edit-feedback-btn {\n  padding: 10px 18px;\n  background: rgba(66, 153, 225, 0.1);\n  color: #4299e1;\n  border: 2px solid rgba(66, 153, 225, 0.2);\n  border-radius: 12px;\n  font-size: 13px;\n  font-weight: var(--font-semibold);\n  cursor: pointer;\n  transition: all 0.3s ease;\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  position: relative;\n  overflow: hidden;\n}\n\n.edit-feedback-btn::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: -100%;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(90deg, transparent, rgba(66, 153, 225, 0.3), transparent);\n  transition: left 0.5s;\n}\n\n.edit-feedback-btn:hover::before {\n  left: 100%;\n}\n\n.edit-feedback-btn:hover {\n  background: rgba(66, 153, 225, 0.2);\n  transform: translateY(-2px);\n  box-shadow: 0 6px 20px rgba(66, 153, 225, 0.2);\n}\n\n.edit-emoji {\n  font-size: 14px;\n  transition: all 0.3s ease;\n}\n\n.edit-feedback-btn:hover .edit-emoji {\n  animation: write 0.8s ease-in-out;\n}\n\n@keyframes write {\n  0%, 100% { transform: rotate(0deg); }\n  25% { transform: rotate(-10deg); }\n  75% { transform: rotate(10deg); }\n}\n\n.feedback-ratings {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 15px;\n  margin-bottom: 15px;\n}\n\n.rating-item {\n  padding: 10px 15px;\n  background: rgba(16, 185, 129, 0.1);\n  color: #059669;\n  border-radius: 12px;\n  font-size: 14px;\n  font-weight: var(--font-semibold);\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  border: 2px solid rgba(16, 185, 129, 0.2);\n  transition: all 0.3s ease;\n}\n\n.rating-item:hover {\n  background: rgba(16, 185, 129, 0.15);\n  transform: translateY(-1px);\n  box-shadow: 0 4px 15px rgba(16, 185, 129, 0.2);\n}\n\n.rating-item.overall {\n  background: rgba(66, 153, 225, 0.1);\n  color: #4299e1;\n  border-color: rgba(66, 153, 225, 0.2);\n}\n\n.rating-item.overall:hover {\n  background: rgba(66, 153, 225, 0.15);\n  box-shadow: 0 4px 15px rgba(66, 153, 225, 0.2);\n}\n\n.rating-item.instructor {\n  background: rgba(139, 69, 19, 0.1);\n  color: #8b4513;\n  border-color: rgba(139, 69, 19, 0.2);\n}\n\n.rating-item.instructor:hover {\n  background: rgba(139, 69, 19, 0.15);\n  box-shadow: 0 4px 15px rgba(139, 69, 19, 0.2);\n}\n\n.rating-item.quality {\n  background: rgba(147, 51, 234, 0.1);\n  color: #9333ea;\n  border-color: rgba(147, 51, 234, 0.2);\n}\n\n.rating-item.quality:hover {\n  background: rgba(147, 51, 234, 0.15);\n  box-shadow: 0 4px 15px rgba(147, 51, 234, 0.2);\n}\n\n.rating-item.recommendation {\n  background: rgba(245, 101, 101, 0.1);\n  color: #f56565;\n  border-color: rgba(245, 101, 101, 0.2);\n}\n\n.rating-item.recommendation:hover {\n  background: rgba(245, 101, 101, 0.15);\n  box-shadow: 0 4px 15px rgba(245, 101, 101, 0.2);\n}\n\n.rating-item .rating-emoji {\n  font-size: 16px;\n  animation: float-gentle 3s ease-in-out infinite;\n}\n\n@keyframes float-gentle {\n  0%, 100% { transform: translateY(0px); }\n  50% { transform: translateY(-2px); }\n}\n\n.feedback-date {\n  font-size: 14px;\n  color: var(--text-secondary);\n  margin: 10px 0;\n}\n\n.feedback-comment {\n  font-style: italic;\n  color: #4a5568;\n  background: rgba(66, 153, 225, 0.05);\n  padding: 15px;\n  border-radius: 12px;\n  border-left: 4px solid #4299e1;\n  margin: 15px 0;\n}\n\n.feedback-details {\n  display: flex;\n  gap: 20px;\n  font-size: 12px;\n  color: #9ca3af;\n  margin-top: 15px;\n  padding-top: 15px;\n  border-top: 1px solid rgba(148, 163, 184, 0.2);\n}\n\n/* Loading State */\n.page-loading {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  min-height: 60vh;\n  font-size: 18px;\n  color: var(--text-accent);\n  text-shadow: 0 2px 10px rgba(0, 0, 0, 0.3);\n}\n\n/* Responsive Design */\n@media (max-width: 768px) {\n  .feedback-page {\n    padding: 15px;\n  }\n  \n  .page-header {\n    flex-direction: column;\n    gap: 20px;\n    text-align: center;\n    padding: 20px;\n  }\n  \n  .page-header h1 {\n    font-size: 24px;\n  }\n  \n  .feedback-section {\n    padding: 25px;\n  }\n  \n  .feedback-item {\n    flex-direction: column;\n    align-items: flex-start;\n    gap: 15px;\n  }\n  \n  .feedback-btn {\n    align-self: stretch;\n    text-align: center;\n  }\n  \n  .modal-content {\n    margin: 10px;\n  }\n  \n  .recommendation-buttons {\n    flex-direction: column;\n  }\n  \n  .form-actions {\n    flex-direction: column;\n  }\n  \n  .feedback-header {\n    flex-direction: column;\n    align-items: flex-start;\n    gap: 10px;\n  }\n  \n  .feedback-actions {\n    align-self: stretch;\n    justify-content: space-between;\n  }\n}\n\n@media (max-width: 480px) {\n  .feedback-section {\n    padding: 20px;\n  }\n  \n  .modal-header {\n    padding: 20px;\n  }\n  \n  .feedback-form {\n    padding: 20px;\n  }\n  \n  .star {\n    font-size: 20px;\n  }\n  \n  .feedback-ratings {\n    flex-direction: column;\n    gap: 10px;\n  }\n}\n\n/* COMPREHENSIVE FIX FOR RESPONSIVE EMPTY STATE - MATCHING DASHBOARD PATTERN */\n/* Higher specificity overrides to ensure proper responsive behavior */\n.feedback-page .feedback-section {\n  display: flex;\n  flex-direction: column;\n  align-items: stretch;\n  width: 100%;\n  box-sizing: border-box;\n}\n\n.feedback-page .feedback-section .empty-state {\n  width: 100% !important;\n  max-width: none !important;\n  margin: 0 !important;\n  padding: 40px 20px;\n  align-self: stretch;\n  box-sizing: border-box;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  text-align: center;\n}\n\n/* Ensure all children also respect the full width */\n.feedback-page .feedback-section .empty-state * {\n  box-sizing: border-box;\n}\n\n/* Theme-aware CTA button styling */\n.feedback-page .feedback-section .empty-state .cta-button {\n  background: var(--btn-secondary-bg, var(--color-primary));\n  border: 1px solid var(--btn-secondary-bg, var(--color-primary));\n}\n\n.feedback-page .feedback-section .empty-state .cta-button:hover {\n  background: var(--btn-secondary-hover, var(--color-primary-hover));\n  border-color: var(--btn-secondary-hover, var(--color-primary-hover));\n}\n\n/* Mobile responsive adjustments with higher specificity */\n@media (max-width: 768px) {\n  .feedback-page .feedback-section .empty-state {\n    padding: 30px 16px;\n  }\n}\n\n@media (max-width: 480px) {\n  .feedback-page .feedback-section .empty-state {\n    padding: 25px 12px;\n  }\n}\n/*# sourceMappingURL=data:application/json;base64, */"],["STYLE",{},"/* Import theme variables from StudentDashboard.css */\n\n/* SessionDetails - With theme support */\n.session-details {\n  min-height: 100vh;\n  background: var(--bg-primary);\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', sans-serif;\n  padding: 20px;\n  transition: background 0.3s ease;\n}\n\n/* Page Header - matches login header style */\n.page-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 30px;\n  color: var(--text-accent);\n}\n\n.page-header h1 {\n  font-size: 28px;\n  font-weight: var(--font-bold);\n  color: var(--text-header);\n  margin: 8px 0;\n}\n\n.back-link {\n  color: var(--text-accent);\n  text-decoration: none;\n  font-size: 14px;\n  transition: color 0.2s ease;\n  font-weight: var(--font-medium);\n}\n\n.back-link:hover {\n  color: #f0f0f0;\n  text-shadow: 0 0 8px rgba(255, 255, 255, 0.5);\n}\n\n.logout-btn {\n  padding: 12px 20px;\n  background: rgba(255, 255, 255, 0.1);\n  color: var(--text-accent);\n  border: 1px solid rgba(255, 255, 255, 0.2);\n  border-radius: 12px;\n  font-size: 14px;\n  cursor: pointer;\n  transition: all 0.2s ease;\n}\n\n.logout-btn:hover {\n  background: rgba(255, 255, 255, 0.2);\n  transform: translateY(-1px);\n}\n\n/* Loading State */\n.page-loading {\n  background: var(--bg-card);\n  border-radius: 20px;\n  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);\n  padding: 60px;\n  text-align: center;\n  color: var(--text-secondary);\n  font-size: 18px;\n  margin: 20px auto;\n  max-width: 500px;\n}\n\n/* Session Not Found */\n.session-not-found {\n  background: var(--bg-card);\n  border-radius: 20px;\n  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);\n  padding: 60px;\n  text-align: center;\n  max-width: 600px;\n  margin: 0 auto;\n}\n\n.session-not-found h2 {\n  color: var(--text-primary);\n  font-size: 24px;\n  font-weight: var(--font-bold);\n  margin-bottom: 15px;\n}\n\n.session-not-found p {\n  color: var(--text-secondary);\n  font-size: 16px;\n  margin-bottom: 30px;\n}\n\n.back-btn {\n  display: inline-block;\n  padding: 16px 32px;\n  background: linear-gradient(135deg, #4299e1, #3182ce);\n  color: var(--text-accent);\n  text-decoration: none;\n  border-radius: 12px;\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  transition: all 0.2s ease;\n}\n\n.back-btn:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 8px 25px rgba(66, 153, 225, 0.3);\n}\n\n/* Notification Banners */\n.error-banner {\n  background: #fed7d7;\n  color: #e53e3e;\n  padding: 12px 16px;\n  border-radius: 12px;\n  font-size: 14px;\n  text-align: center;\n  margin: 20px 0;\n  border: 1px solid #feb2b2;\n}\n\n.success-banner {\n  background: #c6f6d5;\n  color: #2f855a;\n  padding: 12px 16px;\n  border-radius: 12px;\n  font-size: 14px;\n  text-align: center;\n  margin: 20px 0;\n  border: 1px solid #9ae6b4;\n}\n\n/* Session Details Content */\n.session-details-content {\n  background: var(--bg-card);\n  border-radius: 20px;\n  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);\n  padding: 40px;\n  margin-bottom: 20px;\n}\n\n/* Session Header Section */\n.session-header-section {\n  margin-bottom: 30px;\n  padding-bottom: 20px;\n  border-bottom: 2px solid #e2e8f0;\n}\n\n.session-title-area h2 {\n  color: var(--text-primary);\n  font-size: 32px;\n  font-weight: var(--font-bold);\n  margin-bottom: 15px;\n}\n\n.session-status-badges {\n  display: flex;\n  gap: 10px;\n  flex-wrap: wrap;\n}\n\n.status-badge {\n  padding: 8px 16px;\n  border-radius: 8px;\n  font-size: 12px;\n  font-weight: var(--font-semibold);\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.status-badge.available {\n  background: #c6f6d5;\n  color: #2f855a;\n}\n\n.status-badge.full {\n  background: #fed7d7;\n  color: #e53e3e;\n}\n\n.status-badge.past {\n  background: #e2e8f0;\n  color: var(--text-secondary);\n}\n\n.status-badge.confirmed {\n  background: #bee3f8;\n  color: #2b6cb0;\n}\n\n.status-badge.pending {\n  background: #fef3c7;\n  color: #d97706;\n}\n\n.status-badge.cancelled {\n  background: #fed7d7;\n  color: #e53e3e;\n}\n\n/* Content Grid */\n.session-content-grid {\n  display: grid;\n  grid-template-columns: 1fr 400px;\n  gap: 40px;\n  margin-bottom: 30px;\n}\n\n/* Main Details */\n.session-main-details {\n  display: flex;\n  flex-direction: column;\n  gap: 25px;\n}\n\n.detail-group {\n  background: var(--bg-secondary);\n  padding: 20px;\n  border-radius: 12px;\n  border: 2px solid var(--border-color);\n}\n\n.detail-group h3 {\n  color: var(--text-primary);\n  font-size: 18px;\n  font-weight: var(--font-semibold);\n  margin-bottom: 10px;\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.detail-group p {\n  color: #4a5568;\n  font-size: 16px;\n  margin: 0;\n  line-height: var(--leading-relaxed);\n}\n\n.session-datetime {\n  font-size: 18px;\n}\n\n.session-description {\n  background: var(--bg-card);\n  padding: 15px;\n  border-radius: 8px;\n  font-style: italic;\n  color: var(--text-secondary);\n  border-left: 4px solid #4299e1;\n}\n\n/* Capacity Bar */\n.capacity-bar {\n  width: 100%;\n  height: 8px;\n  background: #e2e8f0;\n  border-radius: 4px;\n  margin-top: 10px;\n  overflow: hidden;\n}\n\n.capacity-fill {\n  height: 100%;\n  background: linear-gradient(135deg, #4299e1, #3182ce);\n  transition: width 0.3s ease;\n}\n\n/* Booking Actions Panel */\n.booking-actions-panel {\n  background: var(--bg-card);\n  backdrop-filter: blur(20px);\n  border-radius: 20px;\n  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);\n  padding: 30px;\n  border: 1px solid rgba(255, 255, 255, 0.3);\n  display: flex;\n  flex-direction: column;\n  gap: 25px;\n  height: fit-content;\n  position: sticky;\n  top: 20px;\n  overflow: hidden;\n  position: relative;\n}\n\n.booking-actions-panel::before {\n  content: '';\n  position: absolute;\n  top: -50%;\n  left: -50%;\n  width: 200%;\n  height: 200%;\n  background: radial-gradient(circle, rgba(102, 126, 234, 0.05) 1px, transparent 1px);\n  background-size: 20px 20px;\n  animation: float 15s ease-in-out infinite;\n  pointer-events: none;\n  z-index: -1;\n}\n\n@keyframes float {\n  0%, 100% { transform: translateY(0px) rotate(0deg); }\n  50% { transform: translateY(-10px) rotate(180deg); }\n}\n\n.booking-status h3 {\n  color: var(--text-primary);\n  font-size: 20px;\n  font-weight: var(--font-bold);\n  margin-bottom: 10px;\n}\n\n.booking-status p {\n  color: var(--text-secondary);\n  font-size: 14px;\n  margin: 5px 0;\n}\n\n/* Booking Status Cards with Interactive Emojis */\n.booking-confirmed,\n.booking-unavailable,\n.booking-available {\n  padding: 25px;\n  border-radius: 16px;\n  text-align: center;\n  position: relative;\n  overflow: hidden;\n  transition: all 0.3s ease;\n}\n\n.booking-confirmed {\n  background: linear-gradient(135deg, #c6f6d5, #9ae6b4);\n  border: 2px solid #68d391;\n  box-shadow: 0 8px 25px rgba(72, 187, 120, 0.3);\n}\n\n.booking-confirmed:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 12px 35px rgba(72, 187, 120, 0.4);\n}\n\n.booking-confirmed h3 {\n  color: #22543d;\n  font-weight: var(--font-bold);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 12px;\n}\n\n.booking-confirmed h3::before {\n  content: 'üéâ';\n  font-size: 28px;\n  animation: party-confirmed 3s ease-in-out infinite;\n}\n\n@keyframes party-confirmed {\n  0%, 100% { transform: scale(1) rotate(0deg); }\n  20% { transform: scale(1.3) rotate(-10deg); }\n  40% { transform: scale(1.1) rotate(15deg); }\n  60% { transform: scale(1.25) rotate(-8deg); }\n  80% { transform: scale(1.15) rotate(12deg); }\n}\n\n.booking-unavailable {\n  background: linear-gradient(135deg, #fed7d7, #fecaca);\n  border: 2px solid #fc8181;\n  box-shadow: 0 8px 25px rgba(239, 68, 68, 0.3);\n}\n\n.booking-unavailable:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 12px 35px rgba(239, 68, 68, 0.4);\n}\n\n.booking-unavailable h3 {\n  color: #742a2a;\n  font-weight: var(--font-bold);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 12px;\n}\n\n/* Session Full */\n.booking-unavailable h3::before {\n  content: 'üö´';\n  font-size: 28px;\n  animation: wobble-full 2.5s ease-in-out infinite;\n}\n\n/* Session Completed (Past) - Override the unavailable emoji */\n.session-completed h3::before {\n  content: 'üèÅ';\n  font-size: 28px;\n  animation: checkered-flag 3s ease-in-out infinite;\n}\n\n@keyframes wobble-full {\n  0%, 100% { transform: rotate(0deg) scale(1); }\n  25% { transform: rotate(-15deg) scale(1.1); }\n  50% { transform: rotate(15deg) scale(0.95); }\n  75% { transform: rotate(-10deg) scale(1.05); }\n}\n\n@keyframes checkered-flag {\n  0%, 100% { transform: translateY(0px) rotate(0deg); }\n  25% { transform: translateY(-8px) rotate(-5deg); }\n  50% { transform: translateY(-12px) rotate(5deg); }\n  75% { transform: translateY(-6px) rotate(-3deg); }\n}\n\n.booking-available {\n  background: linear-gradient(135deg, #bee3f8, #90cdf4);\n  border: 2px solid #63b3ed;\n  box-shadow: 0 8px 25px rgba(66, 153, 225, 0.3);\n}\n\n.booking-available:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 12px 35px rgba(66, 153, 225, 0.4);\n}\n\n.booking-available h3 {\n  color: #1a365d;\n  font-weight: var(--font-bold);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 12px;\n}\n\n.booking-available h3::before {\n  content: 'üò¥';\n  font-size: 28px;\n  animation: float-sleepy 20s ease-in-out infinite;\n}\n\n@keyframes float-sleepy {\n  0%, 100% { transform: translateY(0px) rotate(0deg); }\n  50% { transform: translateY(-10px) rotate(180deg); }\n}\n\n/* Action Buttons */\n.action-buttons {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.book-session-btn,\n.cancel-booking-btn,\n.browse-more-btn,\n.my-bookings-btn {\n  width: 100%;\n  padding: 16px;\n  border-radius: 12px;\n  font-size: 16px;\n  font-weight: var(--font-semibold);\n  text-decoration: none;\n  text-align: center;\n  cursor: pointer;\n  transition: all 0.2s ease;\n  border: none;\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  gap: 8px;\n}\n\n.book-session-btn.primary {\n  background: linear-gradient(135deg, #4299e1, #3182ce);\n  color: var(--text-accent);\n  position: relative;\n  overflow: hidden;\n}\n\n.book-session-btn.primary::before {\n  content: 'üöÄ';\n  margin-right: 8px;\n  font-size: 18px;\n  animation: rocket-launch 2.5s ease-in-out infinite;\n}\n\n@keyframes rocket-launch {\n  0%, 100% { transform: translateY(0px) rotate(0deg) scale(1); }\n  25% { transform: translateY(-5px) rotate(-3deg) scale(1.1); }\n  50% { transform: translateY(-10px) rotate(3deg) scale(1.2); }\n  75% { transform: translateY(-3px) rotate(-2deg) scale(1.05); }\n}\n\n.book-session-btn.primary:hover:not(:disabled) {\n  transform: translateY(-3px);\n  box-shadow: 0 12px 35px rgba(66, 153, 225, 0.4);\n}\n\n.book-session-btn.primary:hover:not(:disabled)::before {\n  animation: bounce-book 0.5s ease-in-out infinite;\n}\n\n.cancel-booking-btn.secondary {\n  background: linear-gradient(135deg, #fed7d7, #fecaca);\n  color: #9b2c2c;\n  border: 1px solid #feb2b2;\n  position: relative;\n}\n\n.cancel-booking-btn.secondary::before {\n  content: 'üö®';\n  margin-right: 8px;\n  font-size: 16px;\n  animation: alert-siren 2s ease-in-out infinite;\n}\n\n@keyframes alert-siren {\n  0%, 100% { transform: scale(1) rotate(0deg); opacity: 1; }\n  25% { transform: scale(1.2) rotate(-10deg); opacity: 0.8; }\n  50% { transform: scale(0.9) rotate(10deg); opacity: 1; }\n  75% { transform: scale(1.1) rotate(-5deg); opacity: 0.9; }\n}\n\n.cancel-booking-btn.secondary:hover:not(:disabled) {\n  background: linear-gradient(135deg, #fecaca, #f56565);\n  transform: translateY(-2px);\n  box-shadow: 0 6px 18px rgba(239, 68, 68, 0.3);\n}\n\n.cancel-booking-btn.secondary:hover:not(:disabled)::before {\n  animation: shake-cancel 0.3s ease-in-out infinite;\n}\n\n.browse-more-btn,\n.my-bookings-btn {\n  background: rgba(66, 153, 225, 0.1);\n  color: #4299e1;\n  border: 1px solid rgba(66, 153, 225, 0.2);\n}\n\n.browse-more-btn:hover,\n.my-bookings-btn:hover {\n  background: rgba(66, 153, 225, 0.2);\n  transform: translateY(-1px);\n  text-decoration: none;\n}\n\n.book-session-btn:disabled,\n.cancel-booking-btn:disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n}\n\n/* Session Notes */\n.session-notes {\n  background: #f0fff4;\n  border: 1px solid #9ae6b4;\n  border-radius: 12px;\n  padding: 20px;\n  margin-top: 30px;\n}\n\n.session-notes h3 {\n  color: #2f855a;\n  font-size: 18px;\n  font-weight: var(--font-semibold);\n  margin-bottom: 10px;\n}\n\n.session-notes p {\n  color: #4a5568;\n  font-size: 16px;\n  margin: 0;\n  line-height: var(--leading-relaxed);\n}\n\n/* Responsive Design */\n@media (max-width: 1024px) {\n  .session-content-grid {\n    grid-template-columns: 1fr;\n    gap: 30px;\n  }\n  \n  .booking-actions-panel {\n    position: static;\n  }\n}\n\n@media (max-width: 768px) {\n  .session-details {\n    padding: 15px;\n  }\n  \n  .page-header {\n    flex-direction: column;\n    gap: 15px;\n    text-align: center;\n  }\n  \n  .session-details-content {\n    padding: 25px;\n  }\n  \n  .session-title-area h2 {\n    font-size: 24px;\n  }\n  \n  .session-status-badges {\n    justify-content: center;\n  }\n}\n\n@media (max-width: 480px) {\n  .session-details-content {\n    padding: 20px;\n  }\n  \n  .detail-group {\n    padding: 15px;\n  }\n  \n  .booking-actions-panel {\n    padding: 20px;\n  }\n  \n  .action-buttons {\n    gap: 10px;\n  }\n  \n  .book-session-btn,\n  .cancel-booking-btn,\n  .browse-more-btn,\n  .my-bookings-btn {\n    padding: 14px;\n    font-size: 14px;\n  }\n}\n/*# sourceMappingURL=data:application/json;base64, */"],["STYLE",{},"/* Gamification Profile Styles */\n.gamification-profile {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 2rem;\n  background: var(--bg-primary);\n  min-height: 100vh;\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', sans-serif;\n  transition: background 0.3s ease;\n}\n\n/* Header */\n.profile-header {\n  margin-bottom: 2rem;\n}\n\n.back-button {\n  background: var(--bg-glass);\n  border: 1px solid var(--border-color);\n  color: var(--text-header);\n  padding: 0.75rem 1.5rem;\n  border-radius: 8px;\n  cursor: pointer;\n  font-size: var(--text-sm);\n  transition: all 0.3s ease;\n  margin-bottom: 1rem;\n  backdrop-filter: blur(10px);\n}\n\n.back-button:hover {\n  background: var(--bg-card-hover);\n  transform: translateY(-2px);\n}\n\n.profile-title h1 {\n  color: var(--text-header);\n  font-size: var(--text-4xl);\n  margin: 0;\n  text-shadow: 2px 2px 4px var(--shadow-primary);\n  transition: color 0.3s ease;\n}\n\n.profile-title p {\n  color: var(--text-header);\n  font-size: var(--text-lg);\n  margin: 0.5rem 0 0 0;\n  transition: color 0.3s ease;\n}\n\n/* Player Card */\n.player-card {\n  background: var(--bg-card);\n  backdrop-filter: blur(20px);\n  border-radius: 20px;\n  padding: 2rem;\n  margin-bottom: 2rem;\n  box-shadow: 0 20px 40px var(--shadow-primary);\n  border: 1px solid var(--border-color);\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  gap: 2rem;\n  transition: all 0.3s ease;\n  /* iOS Safari fix for proper containment */\n  position: relative;\n  overflow: hidden;\n  contain: layout style;\n}\n\n.player-info {\n  display: flex;\n  align-items: center;\n  gap: 1.5rem;\n}\n\n.player-avatar {\n  width: 80px;\n  height: 80px;\n  border-radius: 50%;\n  background: var(--gradient-primary);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: var(--text-3xl);\n  font-weight: bold;\n  color: var(--text-accent);\n  box-shadow: 0 8px 16px var(--shadow-secondary);\n}\n\n.player-details h2 {\n  margin: 0;\n  color: var(--text-primary);\n  font-size: var(--text-3xl);\n  transition: color 0.3s ease;\n}\n\n.player-email {\n  color: var(--text-secondary);\n  margin: 0.5rem 0;\n  transition: color 0.3s ease;\n  /* iOS Safari fix for text positioning */\n  position: relative;\n  z-index: 1;\n  word-break: break-word;\n  max-width: 100%;\n}\n\n.veteran-status {\n  margin-top: 0.5rem;\n  /* iOS Safari fix for proper container positioning */\n  position: relative;\n  z-index: 1;\n}\n\n.veteran-badge {\n  background: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);\n  color: white;\n  padding: 0.5rem 1rem;\n  border-radius: 25px;\n  font-size: var(--text-sm);\n  font-weight: bold;\n  display: inline-block;\n  animation: shimmer 2s infinite;\n  box-shadow: 0 4px 8px rgba(240, 147, 251, 0.3);\n  /* iOS Safari fix for red border visibility and positioning */\n  position: relative;\n  z-index: 2;\n  border: 2px solid #f5576c;\n  transform: translateZ(0);\n  -webkit-transform: translateZ(0);\n  will-change: transform;\n}\n\n@keyframes shimmer {\n  0% { box-shadow: 0 4px 8px rgba(240, 147, 251, 0.3); }\n  50% { box-shadow: 0 6px 12px rgba(240, 147, 251, 0.5); }\n  100% { box-shadow: 0 4px 8px rgba(240, 147, 251, 0.3); }\n}\n\n.player-stats {\n  flex: 1 1;\n  max-width: 300px;\n}\n\n.level-section {\n  background: var(--gradient-primary);\n  padding: 1.5rem;\n  border-radius: 15px;\n  color: var(--text-accent);\n}\n\n.level-info {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 1rem;\n}\n\n.level-label {\n  font-size: var(--text-base);\n  opacity: 0.9;\n}\n\n.level-number {\n  font-size: var(--text-4xl);\n  font-weight: bold;\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);\n}\n\n.xp-progress {\n  width: 100%;\n}\n\n.xp-info {\n  display: flex;\n  justify-content: space-between;\n  font-size: var(--text-sm);\n  margin-bottom: 0.5rem;\n  opacity: 0.9;\n}\n\n.progress-bar {\n  width: 100%;\n  height: 10px;\n  background: var(--surface-tertiary);\n  border: 1px solid var(--border-secondary);\n  border-radius: 5px;\n  overflow: hidden;\n  transition: background-color 0.3s ease, border-color 0.3s ease;\n}\n\n.progress-fill {\n  height: 100%;\n  background: var(--bg-gradient, linear-gradient(135deg, #8b5fbf, #667eea));\n  border-radius: 4px;\n  transition: width 0.3s ease, background 0.3s ease;\n  box-shadow: 0 0 12px var(--color-primary, #8b5fbf);\n  position: relative;\n  overflow: hidden;\n}\n\n.progress-fill::after {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: linear-gradient(90deg, \n    transparent 0%, \n    rgba(255, 255, 255, 0.2) 50%, \n    transparent 100%);\n  animation: shine 2s infinite;\n}\n\n@keyframes shine {\n  0% { transform: translateX(-100%); }\n  100% { transform: translateX(100%); }\n}\n\n/* Stats Grid */\n.stats-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\n  gap: 1.5rem;\n  margin-bottom: 2rem;\n}\n\n.stat-card {\n  background: var(--bg-card);\n  backdrop-filter: blur(20px);\n  border-radius: 15px;\n  padding: 1.5rem;\n  display: flex;\n  align-items: center;\n  gap: 1rem;\n  box-shadow: 0 10px 20px var(--shadow-primary);\n  border: 1px solid var(--border-color);\n  transition: all 0.3s ease;\n}\n\n.stat-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 15px 30px var(--shadow-hover);\n}\n\n.stat-icon {\n  font-size: var(--text-4xl);\n  width: 60px;\n  height: 60px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background: var(--gradient-primary);\n  border-radius: 50%;\n}\n\n.stat-info {\n  flex: 1 1;\n}\n\n.stat-value {\n  font-size: var(--text-3xl);\n  font-weight: bold;\n  color: var(--text-primary);\n  margin: 0;\n  transition: color 0.3s ease;\n}\n\n.stat-label {\n  color: var(--text-secondary);\n  font-size: var(--text-sm);\n  margin-top: 0.25rem;\n  transition: color 0.3s ease;\n}\n\n/* Timeline */\n.timeline-section {\n  background: var(--bg-card);\n  backdrop-filter: blur(20px);\n  border-radius: 20px;\n  padding: 2rem;\n  margin-bottom: 2rem;\n  box-shadow: 0 20px 40px var(--shadow-primary);\n  border: 1px solid var(--border-color);\n  transition: all 0.3s ease;\n}\n\n.timeline-section h3 {\n  margin-top: 0;\n  color: var(--text-primary);\n  font-size: var(--text-3xl);\n  margin-bottom: 2rem;\n  text-align: center;\n  transition: color 0.3s ease;\n}\n\n.timeline {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 1rem;\n  justify-content: center;\n}\n\n.timeline-item-wrapper {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 0.5rem;\n}\n\n.semester-index {\n  font-size: var(--text-xs);\n  font-weight: 700;\n  color: var(--color-primary);\n  background: linear-gradient(135deg, var(--bg-card-secondary) 0%, var(--bg-card) 100%);\n  padding: 0.25rem 0.625rem;\n  border-radius: 20px;\n  border: 1px solid var(--color-primary-alpha);\n  box-shadow: 0 2px 8px var(--shadow-light);\n  transition: all 0.3s ease;\n  text-align: center;\n  min-width: 75px;\n  letter-spacing: 0.025em;\n  text-transform: uppercase;\n  backdrop-filter: blur(10px);\n}\n\n.semester-index:hover {\n  transform: translateY(-1px);\n  box-shadow: 0 4px 12px var(--shadow-primary);\n  background: linear-gradient(135deg, var(--color-primary-alpha) 0%, var(--bg-card-secondary) 100%);\n}\n\n.timeline-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 1rem;\n  border-radius: 15px;\n  min-width: 120px;\n  transition: all 0.3s ease;\n}\n\n.timeline-item.completed {\n  background: linear-gradient(135deg, #4CAF50 0%, #8BC34A 100%);\n  color: white;\n}\n\n.timeline-item.current {\n  background: linear-gradient(135deg, #2196F3 0%, #21CBF3 100%);\n  color: white;\n  border: 2px solid #1976D2;\n  box-shadow: 0 4px 15px rgba(33, 150, 243, 0.3);\n}\n\n.timeline-item.upcoming {\n  background: var(--bg-card-secondary);\n  border: 2px dashed var(--border-color);\n  color: var(--text-secondary);\n}\n\n.timeline-marker {\n  font-size: var(--text-3xl);\n  margin-bottom: 0.5rem;\n}\n\n.timeline-content h4 {\n  margin: 0;\n  font-size: var(--text-base);\n}\n\n.timeline-content p {\n  margin: 0.25rem 0 0 0;\n  font-size: var(--text-sm);\n  opacity: 0.8;\n}\n\n/* Achievements */\n.achievements-section,\n.upcoming-section {\n  background: var(--bg-card);\n  backdrop-filter: blur(20px);\n  border-radius: 20px;\n  padding: 2rem;\n  margin-bottom: 2rem;\n  box-shadow: 0 20px 40px var(--shadow-primary);\n  border: 1px solid var(--border-color);\n  transition: all 0.3s ease;\n}\n\n.achievements-section h3,\n.upcoming-section h3 {\n  margin-top: 0;\n  color: var(--text-primary);\n  font-size: var(--text-3xl);\n  margin-bottom: 2rem;\n  text-align: center;\n  transition: color 0.3s ease;\n}\n\n.achievements-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n  gap: 1.5rem;\n}\n\n.achievement-card {\n  border-radius: 15px;\n  padding: 1.5rem;\n  display: flex;\n  align-items: center;\n  gap: 1rem;\n  transition: all 0.3s ease;\n}\n\n.achievement-card.earned {\n  background: linear-gradient(135deg, #FFD700 0%, #FFA500 100%);\n  color: white;\n  box-shadow: 0 10px 20px rgba(255, 215, 0, 0.3);\n}\n\n/* Special styling for new first-time achievements */\n.achievement-card[data-type=\"first_time\"] {\n  border-left: 4px solid #4CAF50;\n  background: linear-gradient(135deg, #4CAF50 0%, #45a049 100%);\n}\n\n.achievement-card[data-type=\"combo\"] {\n  border-left: 4px solid #FF9800;\n  background: linear-gradient(135deg, #FF9800 0%, #F57C00 100%);\n}\n\n.achievement-card[data-type=\"welcome\"] {\n  border-left: 4px solid #9C27B0;\n  background: linear-gradient(135deg, #9C27B0 0%, #7B1FA2 100%);\n}\n\n.achievement-card.upcoming {\n  background: var(--bg-card-secondary);\n  border: 2px dashed var(--border-color);\n  color: var(--text-secondary);\n}\n\n.achievement-card:hover {\n  transform: translateY(-3px);\n}\n\n.achievement-icon {\n  font-size: var(--text-5xl);\n  width: 80px;\n  height: 80px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 50%;\n  background: rgba(255, 255, 255, 0.2);\n}\n\n.achievement-icon.locked {\n  opacity: 0.5;\n  filter: grayscale(100%);\n}\n\n.achievement-info {\n  flex: 1 1;\n}\n\n.achievement-info h4 {\n  margin: 0;\n  font-size: var(--text-lg);\n}\n\n.achievement-info p {\n  margin: 0.5rem 0;\n  opacity: 0.9;\n}\n\n.achievement-date {\n  font-size: var(--text-xs);\n  opacity: 0.8;\n  margin-top: 0.5rem;\n}\n\n.achievement-requirement {\n  font-size: var(--text-sm);\n  font-weight: bold;\n  color: var(--text-primary);\n  margin-top: 0.5rem;\n}\n\n.no-achievements {\n  text-align: center;\n  padding: 3rem;\n  color: var(--text-secondary);\n}\n\n/* Tips Section */\n.tips-section {\n  background: var(--bg-card);\n  backdrop-filter: blur(20px);\n  border-radius: 20px;\n  padding: 2rem;\n  box-shadow: 0 20px 40px var(--shadow-primary);\n  border: 1px solid var(--border-color);\n  transition: all 0.3s ease;\n}\n\n.tips-section h3 {\n  margin-top: 0;\n  color: var(--text-primary);\n  font-size: var(--text-3xl);\n  margin-bottom: 2rem;\n  text-align: center;\n  transition: color 0.3s ease;\n}\n\n.tips-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\n  gap: 1.5rem;\n}\n\n.tip-card {\n  background: var(--gradient-primary);\n  color: var(--text-accent);\n  border-radius: 15px;\n  padding: 1.5rem;\n  display: flex;\n  align-items: flex-start;\n  gap: 1rem;\n  transition: transform 0.3s ease;\n}\n\n.tip-card:hover {\n  transform: translateY(-3px);\n}\n\n.tip-icon {\n  font-size: var(--text-3xl);\n  width: 50px;\n  height: 50px;\n  display: flex;\n  align-items: center;\n  justify-co